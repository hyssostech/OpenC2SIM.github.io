<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
	xmlns="http://www.sisostds.org/schemas/C2SIM/1.1"
	targetNamespace="http://www.sisostds.org/schemas/C2SIM/1.1">

	<!-- repackaging of C2SIM_SMX_LOX_v1.0.1.xsd from C2SIM Core/SMX/LOX Ontology -->
	<!-- replacing ref and group by an equivalent direct use of the element names -->
        <!-- elements from v9 Autonomous Systems Extension (ASX) have been retained -->

	<xs:complexType name="MessageBodyType">
		<xs:choice>
			<xs:element ref="DomainMessageBody"/>
		</xs:choice>
	</xs:complexType>

	<xs:element name="MessageBody" type="MessageBodyType"/>

	<xs:complexType name="DomainMessageBodyType">
		<xs:choice>
			<xs:element name="ReportBody" type="ReportBodyType"/>
		</xs:choice>
	</xs:complexType>
	<xs:element name="DomainMessageBody" type="DomainMessageBodyType"/>

	<xs:simpleType name="UUIDBaseType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9a-z]{8}\-[0-9a-z]{4}\-[0-9a-z]{4}\-[0-9a-z]{4}\-[0-9a-z]{12}"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="FromSenderType">
		<xs:restriction base="UUIDBaseType"/>
	</xs:simpleType>

	<xs:simpleType name="ToReceiverType">
		<xs:restriction base="UUIDBaseType"/>
	</xs:simpleType>

	<xs:simpleType name="IsoDateTimeBaseType">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{4}-[0-9]{2}-[0-9]{2}[T]{1}[0-9]{2}:[0-9]{2}:[0-9]{2}[Z]{1}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IsoDateTimeType">
		<xs:restriction base="IsoDateTimeBaseType"/>
	</xs:simpleType>
	<xs:element name="IsoDateTime" type="IsoDateTimeType"/>
	<xs:complexType name="DateTimeType">
		<xs:annotation>
			<xs:documentation>A date and time in ISO 8601:2000 DateTime Group format yyyy-mm-ddThh:mm:ssZ e.g. 1998-05-12T14:15:00Z. From MSDL simple types.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="IsoDateTime" type="IsoDateTimeType" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="TimeReferenceCodeType">
		<xs:annotation>
			<xs:documentation>Indicates whether a relative time is referenced to the start or the end of an interval time.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="IntervalEndTime"/>
			<xs:enumeration value="IntervalStartTime"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="TimeReferenceCode" type="TimeReferenceCodeType"/>

	<xs:simpleType name="EventReferenceType">
		<xs:restriction base="UUIDBaseType"/>
	</xs:simpleType>
	<xs:element name="EventReference" type="EventReferenceType"/>

	<xs:complexType name="RelativeTimeType">
		<xs:annotation>
			<xs:documentation>Specifies an absolute time relative to the start or end time of an event.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="DelayTimeAmount" type="DurationType" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="TimeReferenceCode" minOccurs="1" maxOccurs="1"/>
			<xs:element ref="EventReference" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="RelativeTime" type="RelativeTimeType"/>

	<xs:complexType name="SimulationTimeType">
		<xs:annotation>
			<xs:documentation>A time measured as a time duration since the time instant of the scenario start.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="DelayTimeAmount" type="DurationType" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="SimulationTime" type="SimulationTimeType"/>

	<xs:complexType name="TimeInstantType">
		<xs:choice>
			<xs:element name="DateTime" type="DateTimeType" />
			<xs:element ref="RelativeTime"/>
			<xs:element ref="SimulationTime"/>
		</xs:choice>
	</xs:complexType>

	<xs:simpleType name="DetectionTypeCodeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Detection"/>
			<xs:enumeration value="Discovery"/>
			<xs:enumeration value="Identification"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="DetectionTypeCode" type="DetectionTypeCodeType"/>

	<xs:complexType name="LevelOfAutonomyType">
		<xs:sequence>
			<xs:element name="LevelOfAutonomyCode" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="LevelOfAutonomy" type="LevelOfAutonomyType"/>
	<xs:simpleType name="LevelOfAutonomyCodeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="LoA-0"/>
			<xs:enumeration value="LoA-1"/>
			<xs:enumeration value="LoA-2"/>
			<xs:enumeration value="LoA-3"/>
			<xs:enumeration value="LoA-4"/>
			<xs:enumeration value="LoA-5"/>
			<xs:enumeration value="LoA-6"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="TacticalAttitudeCodeType">
		<xs:annotation>
			<xs:documentation>The specific operational attitude to be adopted for a mission</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="AWARE"/>
			<xs:enumeration value="CARELESS"/>
			<xs:enumeration value="STEALTH"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="BatteryPercentageType">
		<xs:restriction base="percentageType"/>
	</xs:simpleType>
	<xs:element name="BatteryPercentage" type="BatteryPercentageType"/>

	<xs:simpleType name="LinkPercentageType">
		<xs:restriction base="percentageType"/>
	</xs:simpleType>
	<xs:element name="LinkPercentage" type="LinkPercentageType"/>

	<xs:simpleType name="CurrentTaskType">
		<xs:restriction base="UUIDBaseType"/>
	</xs:simpleType>
	<xs:element name="CurrentTask" type="CurrentTaskType"/>

	<xs:simpleType name="SuggestedTaskType">
		<xs:restriction base="UUIDBaseType"/>
	</xs:simpleType>
	<xs:element name="SuggestedTask" type="SuggestedTaskType"/>

	<xs:simpleType name="OrderIDType">
		<xs:restriction base="UUIDBaseType"/>
	</xs:simpleType>

	<xs:simpleType name="RequestingEntityType">
		<xs:restriction base="UUIDBaseType"/>
	</xs:simpleType>

	<xs:simpleType name="ReportingEntityType">
		<xs:restriction base="UUIDBaseType"/>
	</xs:simpleType>

	<xs:complexType name="OperationalStatusType">
		<xs:annotation>
			<xs:documentation>An enumeration indicating degree of partial capability.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OperationalStatusCode" type="OperationalStatusCodeType" minOccurs="1"
				maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="percentageType">
		<xs:restriction base="xs:nonNegativeInteger">
			<xs:maxInclusive value="100"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="StrengthPercentageType">
		<xs:restriction base="percentageType"/>
	</xs:simpleType>

	<xs:complexType name="StrengthType">
		<xs:annotation>
			<xs:documentation>Representation of the strength of an entity expressed as a percentage of some starting value.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StrengthPercentage" type="StrengthPercentageType" minOccurs="1"
				maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="SubjectEntityType">
		<xs:restriction base="UUIDBaseType"/>
	</xs:simpleType>

	<xs:simpleType name="ConfidenceLevelType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>

	<xs:simpleType name="ReportIDType">
		<xs:restriction base="UUIDBaseType"/>
	</xs:simpleType>

	<xs:simpleType name="UncertaintyIntervalType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>

	<xs:complexType name="ActivityObservationType">
		<xs:annotation>
			<xs:documentation>An observation of the activity of the subject.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ActorReference" type="ActorReferenceType" minOccurs="0"
				maxOccurs="1"/>
			<xs:element name="ConfidenceLevel" type="ConfidenceLevelType" minOccurs="1"
				maxOccurs="1"/>
			<xs:element name="UncertaintyInterval" type="UncertaintyIntervalType" minOccurs="1"
				maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="HealthObservationType">
		<xs:annotation>
			<xs:documentation>An observation of the health state of a subject; for example operational status or resource count.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ActorReference" type="ActorReferenceType" minOccurs="0"
				maxOccurs="1"/>
			<xs:element name="ConfidenceLevel" type="ConfidenceLevelType" minOccurs="1"
				maxOccurs="1"/>
			<xs:element name="UncertaintyInterval" type="UncertaintyIntervalType" minOccurs="1"
				maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="LocationObservationType">
		<xs:annotation>
			<xs:documentation>An observation of the location of a subject.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
                        <xs:element name="ActorReference" type="ActorReferenceType" minOccurs="0"
				maxOccurs="1"/>
			<xs:element name="ConfidenceLevel" type="ConfidenceLevelType" minOccurs="0"
				maxOccurs="1"/>
			<xs:element name="UncertaintyInterval" type="UncertaintyIntervalType" minOccurs="0"
				maxOccurs="0"/>
			<xs:element name="Location" type="LocationType" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="NameObservationType">
		<xs:annotation>
			<xs:documentation>An observation of the unit name of the subject.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ActorReference" type="ActorReferenceType" minOccurs="0"
				maxOccurs="1"/>
			<xs:element name="ConfidenceLevel" type="ConfidenceLevelType" minOccurs="0"
				maxOccurs="1"/>
			<xs:element name="UncertaintyInterval" type="UncertaintyIntervalType" minOccurs="0"
				maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="SIDCStringType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>

	<xs:simpleType name="EntityTypeStringType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>

	<xs:complexType name="APP6-SIDCType">
		<xs:sequence>
			<xs:element name="EntityTypeString" type="EntityTypeStringType" minOccurs="1"
				maxOccurs="1"/>
			<xs:element name="SIDCString" type="SIDCStringType" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="DISCountryCodeType">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>

	<xs:simpleType name="DISDomainCodeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="DomainAir"/>
			<xs:enumeration value="DomainLand"/>
			<xs:enumeration value="DomainOther"/>
			<xs:enumeration value="DomainSpace"/>
			<xs:enumeration value="DomainSubsurface"/>
			<xs:enumeration value="DomainSurface"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DISKindCodeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="KindCulturalFeature"/>
			<xs:enumeration value="KindEnvironmental"/>
			<xs:enumeration value="KindExpendable"/>
			<xs:enumeration value="KindLifeForm"/>
			<xs:enumeration value="KindMunition"/>
			<xs:enumeration value="KindOther"/>
			<xs:enumeration value="KindPlatform"/>
			<xs:enumeration value="KindRadio"/>
			<xs:enumeration value="KindSensorEmitter"/>
			<xs:enumeration value="KindSupply"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="DISCategoryType">
		<xs:annotation>
			<xs:documentation>Values for this property to be taken from the DIS standard, SISO-REF-010-2015</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>

	<xs:simpleType name="DISExtraType">
		<xs:annotation>
			<xs:documentation>Values for this property to be taken from the DIS standard, SISO-REF-010-2015</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>

	<xs:simpleType name="DISSpecificType">
		<xs:annotation>
			<xs:documentation>Values for this property to be taken from the DIS standard, SISO-REF-010-2015</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>

	<xs:simpleType name="DISSubCategoryType">
		<xs:annotation>
			<xs:documentation>Values for this property to be taken from the DIS standard, SISO-REF-010-2015</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>

	<xs:complexType name="DISEntityTypeType">
		<xs:sequence>
			<xs:element name="EntityTypeString" type="EntityTypeStringType" minOccurs="1"
				maxOccurs="1"/>
			<xs:element name="DISCountryCode" type="DISCountryCodeType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DISDomainCode" type="DISDomainCodeType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DISKindCode" type="DISKindCodeType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DISCategory" type="DISCategoryType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DISExtra" type="DISExtraType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DISSpecific" type="DISSpecificType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="DISSubCategory" type="DISSubCategoryType" minOccurs="1" maxOccurs="1"
			/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="EntityTypeType">
		<xs:choice>
			<xs:element name="APP6-SIDC" type="APP6-SIDCType"/>
			<xs:element name="DISEntityType" type="DISEntityTypeType"/>
		</xs:choice>
	</xs:complexType>

	<xs:complexType name="SubjectTypeObservationType">
		<xs:annotation>
			<xs:documentation>An observation of the type of the subject.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ConfidenceLevel" type="ConfidenceLevelType" minOccurs="1"
				maxOccurs="1"/>
			<xs:element name="UncertaintyInterval" type="UncertaintyIntervalType" minOccurs="1"
				maxOccurs="1"/>
			<xs:element name="EntityType" type="EntityTypeType" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ObservationType">
		<xs:choice>
			<xs:element name="ActivityObservation" type="ActivityObservationType"/>
			<xs:element name="HealthObservation" type="HealthObservationType"/>
			<xs:element name="LocationObservation" type="LocationObservationType"/>
			<xs:element name="NameObservation" type="NameObservationType"/>
			<xs:element name="SubjectTypeObservation"/>
		</xs:choice>
	</xs:complexType>
	<xs:element name="Observation" type="ObservationType"/>
	
	<xs:simpleType name="QuantityType">
		<xs:annotation>
			<xs:documentation>Instances of the class have data identifying a numeric value associated with the quantity for that instance.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:double"/>
	</xs:simpleType>
	
	<xs:complexType name="ResourceType">
		<xs:annotation>
			<xs:documentation>This is a resource that can be part of, transferred to, or removed from an object (Entity), but is not an entity itself; therefore it does not have a location, extent, or other physical attributes.
				Resources may include consumable material such as ammunition and fuel; equipment such as weapons, vehicles, and communication devices; and personnel.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EntityType" type="EntityTypeType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Quantity" type="QuantityType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="ResourcesType">
		<xs:annotation>
			<xs:documentation>A level of personnel, equipment, ammunition, fuel, etc. The user must compare this to the full strength of the unit.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Resource" type="ResourceType" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="EntityHealthStatusType">
		<xs:choice>
			<xs:element name="OperationalStatus" type="OperationalStatusType"/>
			<xs:element name="Resources" type="ResourcesType"/>
			<xs:element name="Strength" type="StrengthType"/>
		</xs:choice>
	</xs:complexType>

	<xs:complexType name="PositionReportContentType">
		<xs:sequence>
			<xs:element name="TimeOfObservation" type="TimeInstantType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="EntityHealthStatus" type="EntityHealthStatusType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Location" type="LocationType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="SubjectEntity" type="SubjectEntityType" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ObservationReportContentType">
		<xs:annotation>
			<xs:documentation>This report contains an observation of some aspect of a subject--not necessarily including the location, health, or other facts that are in a position report.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TimeOfObservation" type="TimeInstantType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="Observation" type="ObservationType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="AutonomousSystemObservationReportContent" 
				type="AutonomousSystemObservationReportContentType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="ObservationReportContent" type="ObservationReportContentType"/>

	<xs:complexType name="AutonomousSystemPositionReportContentType">
		<xs:sequence>
			<xs:element name="LevelOfAutonomyCode" minOccurs="0" maxOccurs="1"/>
			<xs:element name="TacticalAttitudeCode" minOccurs="0" maxOccurs="1"/>
			<xs:element name="BatteryPercentage" minOccurs="0" maxOccurs="1"/>
			<xs:element name="LinkPercentage" minOccurs="0" maxOccurs="1"/>
			<xs:element name="CurrentTask" minOccurs="0" maxOccurs="1"/>
			<xs:element name="SuggestedTask" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="AutonomousSystemPositionReportContent"
		type="AutonomousSystemPositionReportContentType"/>

	<xs:complexType name="AutonomousSystemObservationReportContentType">
		<xs:sequence>
			<xs:element name="DetectionTypeCode" minOccurs="0" maxOccurs="1"/>
			<xs:element name="UUID" minOccurs="0" maxOccurs="1"/>
			<xs:element name="SideHostilityCode" minOccurs="0" maxOccurs="1"/>
			<xs:element name="EntityType" minOccurs="0" maxOccurs="1"/>
			<xs:element name="OperationalStatusCode" minOccurs="0" maxOccurs="1"/>
			<xs:element name="Strength" minOccurs="0" maxOccurs="1"/>
			<xs:element name="CurrentTask" minOccurs="0" maxOccurs="1"/>
			<xs:element name="SuggestedTask" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="TaskStatusType">
		<xs:annotation>
			<xs:documentation>The status of a task, provided before, during or after a task is executed.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TimeOfObservation" type="TimeInstantType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="TaskStatusCode" type="TaskStatusCodeType" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:simpleType name="TaskStatusCodeType">
		<xs:annotation>
			<xs:documentation>An enumeration of possible status values for a task.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="TASKABRT"/>
			<xs:enumeration value="TASKCMPLT"/>
			<xs:enumeration value="TASKINPRG"/>
			<xs:enumeration value="TASKPEND"/>
			<xs:enumeration value="TASKSTRT"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="ReportContentType">
		<xs:choice>
			<xs:element name="PositionReportContent" type="PositionReportContentType"/>
			<xs:element name="TaskStatus" type="TaskStatusType"/>
			<xs:element name="ObservationReportContent" type="ObservationReportContentType"/>
		</xs:choice>
	</xs:complexType>

	<xs:complexType name="ReportBodyType">
		<xs:annotation>
			<xs:documentation>The structure for sending report messages.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FromSender" type="FromSenderType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="ToReceiver" type="ToReceiverType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="ReportContent" type="ReportContentType" minOccurs="1"
				maxOccurs="unbounded"/>
			<xs:element name="ReportID" type="ReportIDType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="ReportingEntity" type="ReportingEntityType" minOccurs="1"
				maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:simpleType name="FunctionalAssociationWithTaskType">
		<xs:annotation>
			<xs:documentation>This property identifies the UUID of a Maneuver Warfare Task that is the object of a JC3IEDM ActionFunctionalAssociation.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="UUIDBaseType"/>
	</xs:simpleType>
	
	<xs:complexType name="TaskFunctionalRelationType">
		<xs:annotation>
			<xs:documentation>A functional relationship between maneuver warfare tasks.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TaskFunctionalAssociationCode" type="TaskFunctionalAssociationCodeType" minOccurs="1" maxOccurs="1"/>
			<xs:element name="FunctionalAssociationWithTask" type="FunctionalAssociationWithTaskType" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	
	<xs:simpleType name="EventCodeType">
		<xs:annotation>
			<xs:documentation>The type of event.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="GenericEvent"/>
			<xs:enumeration value="TaskEnd"/>
			<xs:enumeration value="TaskStart"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:simpleType name="TaskActionCodeType">
		<xs:annotation>
			<xs:documentation>The activity to be performed in the task, e.g. Move, Observe, Assist.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="AssistOtherUnit"/>
			<xs:enumeration value="HoldInPlace"/>
			<xs:enumeration value="MoveToLocation"/>
			<xs:enumeration value="Observe"/>
			<xs:enumeration value="OrientToLocation"/>
			<xs:enumeration value="ReportPosition"/>
			<xs:enumeration value="UseCapability"/>
			<xs:enumeration value="ExecutePlanPhase"/>
		</xs:restriction>
	</xs:simpleType>
	
	<xs:complexType name="ActionCodeType">
		<xs:choice>
			<xs:element name="EventCode" type="EventCodeType"/>
			<xs:element name="TaskActionCode" type="TaskActionCodeType"/>
