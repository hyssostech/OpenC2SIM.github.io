	<!-- Meteorologic Feature Types -->
	<xs:complexType name="Atmosphere">
		<xs:annotation>
			<xs:documentation xml:lang="en">A METEOROLOGIC-FEATURE that specifies humidity, pressure, and temperature characteristics of Earth's atmosphere.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMeteorologicFeature">
				<xs:sequence>
					<xs:element name="HumidityRatio" type="jc3iedm:RatioType6_5" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric quotient value that represents the proportion of water present in the air to the maximum amount of water (saturation point) possible at a given temperature and pressure. The value must be in the range from 0 to 1.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InversionLayerCode" type="jc3iedm:AtmosphereInversionLayerCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the height of the inversion layer in the atmosphere. The stability class describes the degree of mixing of released material in the atmosphere.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PressureQuantity" type="jc3iedm:QuantityType8_4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that denotes the ambient air pressure in terms of force per unit area. The unit of measure is kPa (Kilopascals).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PressureSystemCategoryCode" type="jc3iedm:AtmospherePressureSystemCategoryCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of a pressure system in a particular ATMOSPHERE.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Temperature" type="jc3iedm:TemperatureTypeRangeTemperature5_1" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that indicates the heat of the ambient air for a specific ATMOSPHERE.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TemperatureGradientCode" type="jc3iedm:AtmosphereTemperatureGradientCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents heat change with respect to the ground and 100 m in elevation in a certain area. Acts as an indication of vertical air movement between the ground and higher elevations.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AtmosphereRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Atmosphere - A METEOROLOGIC-FEATURE that specifies humidity, pressure, and temperature characteristics of Earth's atmosphere.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMeteorologicFeatureRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Light">
		<xs:annotation>
			<xs:documentation xml:lang="en">A METEOROLOGIC-FEATURE that specifies the availability of natural illumination by type and time.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMeteorologicFeature">
				<xs:sequence>
					<xs:element name="LightCategoryCode" type="jc3iedm:LightCategoryCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of LIGHT.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="UpDatetime" type="jc3iedm:DatetimeTypeFix18" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The character string representing a point in time that designates the time of day that marks the beginning of the period of effectiveness of the specified type of LIGHT.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DownDatetime" type="jc3iedm:DatetimeTypeFix18" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The character string representing a point in time that designates the time of day that marks the end of the period of effectiveness of the specified type of LIGHT.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MoonPhaseCode" type="jc3iedm:LightMoonPhaseCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the phase of the moon for a specific LIGHT.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="LightRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Light - A METEOROLOGIC-FEATURE that specifies the availability of natural illumination by type and time.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMeteorologicFeatureRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OtherMeteorologicFeature">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FEATURE that describes reported or forecast weather and light conditions.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMeteorologicFeature">
				<xs:sequence>
					<xs:element name="MeteorologicFeatureCategoryCode" type="jc3iedm:MeteorologicFeatureCategoryCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of METEOROLOGIC-FEATURE. It serves as a discriminator that partitions METEOROLOGIC-FEATURE into subtypes.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OtherMeteorologicFeatureRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some OtherMeteorologicFeature - A FEATURE that describes reported or forecast weather and light conditions.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMeteorologicFeatureRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Wind">
		<xs:annotation>
			<xs:documentation xml:lang="en">A METEOROLOGIC-FEATURE that specifies the velocity and directional characteristics of atmospheric movement.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMeteorologicFeature">
				<xs:sequence>
					<xs:element name="WindCategoryCode" type="jc3iedm:WindCategoryCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of WIND.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AirStabilityCategoryCode" type="jc3iedm:WindAirStabilityCategoryCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value used to indicate the class of air stability.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AltitudeLayerCode" type="jc3iedm:WindAltitudeLayerCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value used to indicate the class of the altitude for a specific set of reported wind data.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DirectionAngle" type="jc3iedm:AngleTypeRangeAngle7_4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The rotational measurement clockwise between the line of true North and the direction from which a specific WIND originates.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EffectiveDownwindDirectionAngle" type="jc3iedm:AngleTypeRangeAngle7_4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The rotational measurement of the mean downwind direction at ground level in the hazard area towards which the cloud travels.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpeedRate" type="jc3iedm:RateType8_4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that denotes the distance per unit time of a specific WIND. The unit of measure is kilometres per hour. The specified value must be greater than or equal to 0 (zero).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NuclearYieldQualifierCode" type="jc3iedm:NuclearYieldGroupCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value used to identify predicted wind values associated with nuclear fallout prediction for a specific Nuclear yield group.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="WindRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Wind - A METEOROLOGIC-FEATURE that specifies the velocity and directional characteristics of atmospheric movement.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMeteorologicFeatureRef"/>
		</xs:complexContent>
	</xs:complexType>
