	<xs:simpleType name="RunwayPavementEvaluationMethodCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that represents the pavement evaluation method classification, which is part of the standard ICAO (International Civil Aviation Organization) method of reporting pavement strength for pavements with bearing strength greater than 5,700 kilograms (12,500 pounds).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			
			<xs:enumeration value="U">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specific value, which indicates that the pavement evaluation method is determined by experience of aircraft using the pavement.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">By experience</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="T">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specific value, which indicates that the pavement evaluation method is based on a technical study.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Technical evaluation</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="RunwayPavementMaximumTyrePressureCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that represents the pavement maximum tyre pressure classification, which is part of the standard ICAO (International Civil Aviation Organization) method of reporting pavement strength for pavements with bearing strength greater than 5,700 kilograms (12,500 pounds).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			
			<xs:enumeration value="W">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specific value, which indicates that the maximum tyre pressure is not limited.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">High</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Y">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specific value, which indicates that the maximum tyre pressure is limited between 510,212 Pa and 999,739 Pa (74 and 145 (Pounds per square inch)) PSI (1 PSI = 6894.757 Pa).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Low</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="X">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specific value, which indicates that the maximum tyre pressure is limited between 1006,634 Pa and 1496,162 Pa (146 and 217 (Pounds per square inch)) PSI (1 PSI = 6894.757 Pa).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Medium</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Z">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specific value, which indicates that the maximum tyre pressure is limited to 503,317 Pa (73 Pounds per square inch) PSI (1 PSI = 6894.757 Pa).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Ultra-low</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="RunwayPavementSubgradeCategoryCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that represents the pavement subgrade classification, which is part of the standard ICAO (International Civil Aviation Organization) method of reporting pavement strength for pavements with bearing strength greater than 5,700 kilograms (12,500 pounds).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specific value that indicates that the pavement subgrade is characterised by a (spring constant) K value greater than 400 pci (pavement condition index) for rigid pavements, and by a (California Bearing Ratio) CBR value above 13% for flexible pavements.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">High</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C">
