					<xs:appinfo>
						<DisplayValue xml:lang="en">Great circle route</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INGRTE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An inbound route to a specific area or target.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Ingress route</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
