	<xs:simpleType name="VerticalDistanceReferenceCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that represents the reference system for a specific VERTICAL-DISTANCE.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			
			<xs:enumeration value="CHADAT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The vertical reference established as the lowest observed or theoretical sea level possible for the area, such as Mean Lower Low Water (MLLW) or Lowest Astronomical Tide (LAT).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Chart datum</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LOCDAT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The vertical reference for a level, a point or an object considered as a point measured from a specific datum.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Local datum</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MNSLVL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The vertical reference established as the average level of the ocean surface measured over a full metonic tidal cycle of 18.6 years.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Mean sea level</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PDQFE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The VERTICAL-DISTANCE reference in terms of atmospheric pressure indicated by a pressure altimeter that is calibrated to read 0 at airfield runway ground level.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Pressure datum QFE</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PDQNH">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The VERTICAL-DISTANCE reference in terms of atmospheric pressure indicated by a pressure altimeter that is calibrated to read 0 at Mean Sea Level (MSL).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Pressure datum QNH</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PDSTDT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The VERTICAL-DISTANCE reference in terms of atmospheric pressure indicated by a pressure altimeter that is calibrated to read 0 at standard atmosphere pressure of 1013.2 millibars.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Pressure datum standard atmosphere</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TOPOSR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The vertical reference is the actual visible surface. This includes both terrain and water surfaces.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Topographic surface</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WATBOT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The datum for VERTICAL-DISTANCE provided by the submerged surface at a point or area.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Water bottom</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WGS84G">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The vertical reference established as the surface of the WGS84 geoid.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">WGS84 geoid</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WGS84R">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The vertical reference established as the surface of the WGS84 ellipsoid.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">WGS84 reference ellipsoid</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

