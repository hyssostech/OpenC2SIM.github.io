	<xs:simpleType name="LanguageSkillProficiencyCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that represents the level of skill or comprehension of a specific PERSON in a specific language skill.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			
			<xs:enumeration value="0">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">No significant or practical proficiency.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">0</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="1">
				<xs:annotation>
