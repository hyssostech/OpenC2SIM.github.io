					<xs:appinfo>
						<DisplayValue xml:lang="en">Utility flight</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VERIFY">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To testify to, to assert, to affirm or confirm, as true or certain.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Verify</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WLDWSL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To identify, locate, and physically suppress or destroy ground based enemy air defence systems that employ sensors radiating electromagnetic energy.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Wild weasel</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WITHDR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To disengage a force in contact from an enemy force.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Withdraw</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WDRPRS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To disengage from the enemy when the enemy has sufficient contact with friendly forces to interfere with the withdrawal.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Withdraw under pressure</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WITNES">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To observe an activity in an official capacity with a view of providing evidence.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Witness</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
