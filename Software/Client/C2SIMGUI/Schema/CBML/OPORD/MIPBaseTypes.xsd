					<xs:documentation>
						<Definition xml:lang="en">A UNIT-TYPE whose designation indicates employment of fixed-wing assets in the air regime.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Aviation, fixed wing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AVARW">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A UNIT-TYPE whose designation indicates employment of rotary-wing assets in the air regime.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Aviation, rotary wing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
