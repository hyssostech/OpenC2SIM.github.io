			<xs:documentation xml:lang="en">The specific value that represents the class of GEOGRAPHIC-FEATURE-TYPE.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			
			<xs:enumeration value="CSTLHY">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Natural features representing different aspects of interchange between water and land.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Coastal hydrography</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CONTNT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Any of the world's main continuous expanses of land.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Continent</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INLNDW">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A sea, lake, river, etc situated in the interior of a land mass.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Inland water</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LNDFRM">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Natural features of the earth's surface.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Landform</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NKN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">It is not possible to determine which value is most applicable.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Not known</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NOS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The appropriate value is not in the set of specified values.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Not otherwise specified</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SNOWIC">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Natural features that are characterised by frozen water.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Snow/ice</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WETLND">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A saturated area, at times covered with water.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Wetland</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="GeographicFeatureTypeSubcategoryCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that represents the detailed class of GEOGRAPHIC-FEATURE-TYPE.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			
			<xs:enumeration value="BCKSHR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Area between the normal limit of wave action above either Mean High Water Springs (MHWS) or Mean High High Water (MHHW) as defined by the National Authority and the maximum limit of wave action.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Backshore</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BEACH">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The shore of the sea or lake, sandy or pebbly, brought up by the waves (including the foreshore area).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Beach</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BLUFF">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A steep, vertical, or overhanging face of rock or earth.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Bluff/cliff/escarpment</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BOG">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A poorly drained or periodically flooded area, excluding tidal waters, with soil rich in plant residue.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Bog</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CAVE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A natural subterranean chamber or series of chambers open to the Earth’s surface.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Cave</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CAY">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A small insular feature, which may contain scant vegetation; usually composed of sand or coral. Often applied to smaller coral shoals.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Cay</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CHANEL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">That part of a body of water, sometimes dredged, deep enough for navigation through an area otherwise not navigable. It is usually marked by a single or double line of navigational aids.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Channel</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CSTLNE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A line generally following the contact between a land mass and a body of water, based on the low water line, and used as a reference for measuring international boundaries such as Territorial Limits or Exclusive Economic Zone (EEZ).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Coastline</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CRVCEC">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A narrow fissure, crack, or rift in the Earth’s surface, snow or ice.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Crevice/crevasse</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CUT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An excavation of the Earth’s surface to provide passage for a road, railroad, canal, etc.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Cut</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DPRSSN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A low area surrounded by higher ground.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Depression</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DRYGAP">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A waterless ravine or mountain pass.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Dry gap</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EMBANK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A raised long mound of earth or other material.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Embankment/fill</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ESKER">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A long, narrow ridge of sand and gravel deposited by a glacial stream.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Esker</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FAN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A gently sloping fan shaped feature usually found near the lower termination of a canyon.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Fan</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FAULT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A fracture in the Earth’s crust with displacement on one side of the fracture relative to the other.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Fault</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FLDDAR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Land subject to controlled inundation (i.e. flooded by the regulation of the level of water impounded by a dam or beaver dam), and is normally associated with permanently flooded areas in which trees are still standing. Also known as inundated land.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Flooded area</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FLUME">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An open, inclined channel which carries water for use in such operations as mining or logging.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Flume</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FORD">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A shallow part of a body of water that can be crossed without bridging, boats, or rafts. A location in a water barrier where the physical characteristics of current, bottom, and approaches permit the passage of personnel and/or vehicles and other equipment that remain in contact with the bottom.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Ford</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FRESHR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Area between the low water line defined by Mean Low Water Springs (MLWS) or Mean Low Low Water (MLLW) as appropriate and the normal limit of wave action above Mean High Water Springs (MHWS) or Mean High High Water (MHHW) as appropriate.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Foreshore</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GEOTHR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A terrain surface feature controlled by or derived from the heat of the Earth’s interior.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Geothermal feature</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GLACER">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A large mass of snow and ice moving slowly down a slope or valley from above the snowline.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Glacier</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GULLY">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A long, narrow, deep erosion with steep banks.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Gully/gorge</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HRBRNT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A place of shelter for ships; where they may lie close to and sheltered by the shore or by works extended from it.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Harbour, natural</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HILL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A small, isolated elevation, smaller than a mountain.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Hill</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="HUMOCK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An area of higher elevation within a swamp, bog, or marsh.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Hummock</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ICECLF">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The vertical face of a glacier or ice shelf.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Ice cliff</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ICEPKN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A rocky peak projecting above a surrounding ice field that may be perpetually covered with ice.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Ice peak/nunatak</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ICESHL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A sheet of thick ice, with level or undulating surface, attached to the land but mostly afloat which is bounded on the seaward side by an Ice Cliff.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Ice shelf</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INLNDS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The land-water boundary for all inland hydrographic features having shorelines, Lake /Pond, or Island.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Inland shoreline</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ISLAND">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A land mass smaller than a continent and surrounded by water.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Island</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LAGOON">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Open body of water separated from the sea by a sand bank or coral reef.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Lagoon/reef pool</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LKEPND">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A body of water surrounded by land.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Lake/pond</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LNDSBJ">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An area periodically covered by flood water, excluding tidal waters.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Land subject to inundation</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LNDSLD">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A mass of land, with a high potential of slipping down from a mountain, hill, etc.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Landslide/scree</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LRGRCK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A conspicuous isolated rocky formation or single large stone existing in its entirety above the high water mark. From offshore it would appear to a mariner as a single point on land and would be appropriate for use in navigation.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Large isolated rock, boulder, or rocky formation</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LEDGE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A narrow, flat surface or shelf, especially one that projects, as from a wall of rock.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Ledge</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MARSH">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An area of wet, often spongy ground that is subject to frequent or tidal inundations, but not considered to be continually under water.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Marsh</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MORANE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An accumulation of soil and stone debris deposited by a glacier.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Moraine</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MOUNTN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A natural elevation of the earth's surface rising more or less abruptly from the surrounding level, and attaining an altitude which, relatively to adjacent elevations, is impressive or notable.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Mountain</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MNTPSS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A natural route through a low place in a mountain range.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Mountain pass</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NRSHRE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Area between the 10 meter depth curve and the low water line defined by either Mean Low Water Springs (MLWS) or Mean Low Low Water (MLLW) as defined by the National Authority and the International Hydrographic Organisation.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Nearshore</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PCKICE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An area of ice formed by the drifting and crushing together of floating pieces of ice.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Pack ice</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PINGO">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A cone or dome shaped mound or hill of peat or soil, usually with a core of ice. It is found in tundra regions and is produced by the pressure of water or ice accumulating underground and pushing upward.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Pingo</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PLRICE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The heaviest, thickest form of ice over land or water.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Polar ice</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RAPIDS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A place in a stream or river where the current is swift and the surface is usually broken by boulders and rocks.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Rapids</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="REEF">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A rocky or coral elevation at or near enough to the surface of the sea to be a danger to surface navigation.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Reef</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RVRBNK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The limit line between the water area of a river and the area of land.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">River bank</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RVRSTR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A natural flowing watercourse.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">River/stream</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RVRSTV">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Point at which a river or stream passes into the ground.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">River/stream vanishing point</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ROCK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An isolated rocky formation or a single large stone above or below the water surface.</Definition>
					</xs:documentation>
