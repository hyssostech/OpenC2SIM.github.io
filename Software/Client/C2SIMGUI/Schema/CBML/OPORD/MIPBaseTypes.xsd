	<xs:simpleType name="ActionTaskStartQualifierCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that denotes the role of starting date and time with respect to the period of effectiveness of a specific ACTION-TASK.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="AFT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Time intended is later than the time specified.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">After</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ASAP">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Begin the activity at the earliest possible time once execution is authorised.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">As soon as possible</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ASAPAF">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Begin the activity at the earliest possible time after the specified start time.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">As soon as possible after</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ASAPNL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Begin the activity at the earliest possible time but not later than the specified start time.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">As soon as possible not later than</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Time intended is the time specified.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">At</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BEF">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Time intended is in advance of the time specified.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Before</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NLT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Time specified is the latest.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">No later than</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NOB">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Time specified is the earliest.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Not before</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ONCALL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Time will be specified later by a dedicated call.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">On call</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ONCDWD">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Begin the activity on receipt of codeword.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">On codeword</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TBD">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Time intended is to be determined later.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">To be determined</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UNK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Time intended is not known.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Unknown</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

