					<xs:appinfo>
						<DisplayValue xml:lang="en">Plan</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ActionTaskStatusPlanningIndicatorCodeXmlType">
		<xs:annotation>
