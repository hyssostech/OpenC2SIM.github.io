					<xs:documentation>
						<Definition xml:lang="en">The third highest classification that denotes those ACTION-TASKs that require combat support training (e.g., flare drops, unconventional warfare activities, and Joint Airborne Communications Centre/Command Post (JACC/CP)).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Priority 3B2</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3B3">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The third highest classification that denotes those ACTION-TASKs that are service schools requiring airborne, airdrop, and air transportability training as a part of the training program.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Priority 3B3</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="3B4">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The third highest classification that denotes those ACTION-TASKs that require airdrop/air transportability or aircraft certification of new or modified equipment.</Definition>
					</xs:documentation>
