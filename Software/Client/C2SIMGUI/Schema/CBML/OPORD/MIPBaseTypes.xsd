					<xs:appinfo>
						<DisplayValue xml:lang="en">Temporary minimum risk route</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TRSTRT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A temporary corridor of defined dimensions established in the forward area to minimise the risk to friendly aircraft from friendly air defences or surface forces.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Transit route</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TRNKRT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Trunk route.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Trunk route</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UNMVRT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The CONTROL-FEATURE-TYPE that specifies a route along which an unmanned aerial vehicle travels.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Unmanned aerial vehicle route</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="USCTRT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The route that follows the shape or boundary of a coast.</Definition>
