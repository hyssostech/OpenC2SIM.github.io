						<DisplayValue xml:lang="en">DCT</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EGRRTU">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An outbound route from a specific area or target.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Egress route</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FLTPTH">
				<xs:annotation>
