						<DisplayValue xml:lang="en">Clear icing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MIXICE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A hard rough conglomerate of ice that can cause very rough accumulation and severe loss of lift.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Mixed icing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RIMICE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Rough, milky opaque ice formed by the instantaneous freezing of small supercooled droplets which trap air within the ice as they strike the aircraft.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Rime icing</DisplayValue>
					</xs:appinfo>
