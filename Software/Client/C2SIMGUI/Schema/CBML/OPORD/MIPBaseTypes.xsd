					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Horse</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LAND">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specified UNIT-TYPE moves generally on the ground.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Land</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LNDRAI">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specified UNIT-TYPE moves generally on the ground, along rails.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Land, railed</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LNDTWD">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The main equipment of the specified UNIT-TYPE moves generally on the ground by means of external propulsion (mechanical or animal).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Land, towed</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LNDTRC">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specified UNIT-TYPE moves generally on the ground by means of vehicles using caterpillar treads.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Land, tracked</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LNDWHL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specified UNIT-TYPE moves generally on the ground by means of vehicles using wheels.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Land, wheeled</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

