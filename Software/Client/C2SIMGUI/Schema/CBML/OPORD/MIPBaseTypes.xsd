						<DisplayValue xml:lang="en">T1-Very seriously injured</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="T2">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Seriously injured - 2 hours expected duration for surgery.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">T2-Seriously injured</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
