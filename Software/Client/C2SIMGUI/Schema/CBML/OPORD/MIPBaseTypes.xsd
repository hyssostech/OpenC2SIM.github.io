					<xs:appinfo>
						<DisplayValue xml:lang="en">No operation</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NKN">
				<xs:annotation>
					<xs:documentation>
