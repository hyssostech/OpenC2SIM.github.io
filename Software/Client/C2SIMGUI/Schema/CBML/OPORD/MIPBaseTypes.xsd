					<xs:documentation>
						<Definition xml:lang="en">Providing movement of cargo from one ship or train or container to another for further shipment.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Providing transhipment services</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PRXBMB">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A deliberate and intentional bombing incident that happens unexpectedly where the perpetrator acts through a representative.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Proxy-bombing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PSYOP">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Planned operations to convey selected information and indicators to foreign audiences to influence their emotions, motives, objective reasoning, and ultimately the behaviour of foreign governments, organisations, groups, and individuals. The purpose of psychological operations is to induce or reinforce foreign attitudes and behaviour favourable to the originator's objectives.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Psychological operation</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PUBMDA">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Making generally known a non-fictional essay, especially one included with others in a newspaper, magazine, journal, etc.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Publishing media article</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PUBMDD">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Making generally known any document published on a media that provides a factual record or report.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Publishing media documentary</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PUBPRS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Making generally known an official statement issued to media for information.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Publishing press release</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PURSNG">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Continuing an offensive operation in order to catch or cut off a hostile force attempting to escape, with the aim of destroying it. Typically, contact is maintained and risk taken to harass relentlessly, thereby turning the pursuit into a rout.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Pursuing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RAPE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The act of forcing another person to submit to sexual intercourse.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Rape</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RECCE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Conducting a mission to obtain by visual operations or other detection methods information about the activities and resources of an enemy or potential enemy, or to secure data concerning the meteorological, hydrographic or geographic characteristics of a particular area.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Reconnaissance</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RECCEF">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Conducting an offensive operation designed to discover and/or test the enemy’s strength, or to obtain other information.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Reconnaissance in force</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RECNSN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Attaining prescribed strength of units and prescribed levels of vehicles, equipment, stores and supplies. The process will only take place after a unit/formation combat effectiveness has been reduced.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Reconstituting</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RECVRN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Retrieving any lost, incapacitated or captured object.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Recovering</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RECPRN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Resting a unit after it has been in action. Some reconstitution may take place as well.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Recuperating</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="REDPLN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Transferring a unit, an individual, or supplies deployed in one area to another area, or to another location within the area, for the purpose of further employment.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Redeployment</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="REFMVM">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The movement of people who has been forced to leave their country in order to escape war, persecution, or natural disaster.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Refugee movement</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="REINFN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Making a force available for the purpose of supplementing an in-place force.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Reinforcing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RLFPLC">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An operation in which, by direction of higher authority, all or part of a unit is replaced in an area by the incoming unit. The responsibilities of the replaced elements for the mission and the assigned zone of operations are transferred to the incoming unit. The incoming unit continues the operation as ordered.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Relief in place</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RELDEM">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A public display of group feelings towards a religious idea, person or cause.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Religious demonstration</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RELVIO">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Individual or organised act directed against groups or individuals because of their religious beliefs.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Religious violence</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RELWAR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An act of open armed conflict due to a difference of religious belief between two separate groups.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Religious warfare</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RNDZVS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Achieving a pre-arranged meeting at a given time and place.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Rendezvous</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="REORGN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Changing a task organisation for a particular operation. (Normally takes place before an operation). This includes the transfer of authority.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Reorganising</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="REPAIR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Restoring an item to serviceable condition through correction of a specific failure or unserviceable condition.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Repairing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RESTNG">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Observing a specified period of inactivity by an organisation that is out of contact with the enemy.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Resting</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RESPLN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Replenishing stocks in order to maintain the required levels of supply.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Resupplying</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RETAIN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Occupying and holding a terrain feature to ensure it is free of enemy occupation or use.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Retain</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RETIRE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Moving a force out of contact with the enemy with the expectation of no further significant contact.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Retire</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="REVOLU">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The overthrow or renunciation of one government or ruler and the substitution of another by the governed.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Revolution</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RIOT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A disturbance of the peace by a crowd; an occurrence of public disorder.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Riot</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ROBERY">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Unlawfully taking property, valuables or money from a person or place.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Robbery</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RKTFIR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The employment of a rocket powered weapon.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Rocket fire</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SABOTG">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An act or acts with intent to injure, interfere with, or obstruct the national defence of a country by wilfully injuring or destroying, or attempting to injure or destroy, any national defence or war material, premises or utilities, to include human and natural resources.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Sabotage</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SCRNNG">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Operating as a security element whose primary task is to observe, identify and report information, and which only fights in self-protection.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Screening</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SECCOU">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The formal withdrawal of a portion of a country.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Secession of portion of country</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SECRNG">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Gaining possession of a position or terrain feature, with or without force, and making such disposition to prevent, as far as possible, its destruction or loss by enemy action.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Securing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SECCMP">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A release of information to someone unauthorised.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Security compromise</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SECVIO">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An infringement of a security protocol.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Security violation</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SEIZNG">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Clearing a designated area and obtaining control of it.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Seizing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SRVBRK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Operating as a force that is tasked with the continuation of the operation during an obstacle crossing.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Serving as a breakout force</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SRVBRD">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Operating as a force that seizes or controls ground in order to permit the continuous embarkation, landing or crossing of troops or materiel and to provide manoeuvre space during a water obstacle crossing.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Serving as a bridgehead force</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SRVFLK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Operating as a security element whose primary task is to protect the main force by fighting on the designated flank to gain time, whilst also observing and reporting information.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Serving as a flank guard</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SRVMNB">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Operating as the main force for a specific operation.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Serving as a main body</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SRVRGD">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Operating as a security element whose primary task is to move (or remain) at the rear of the main body and protect the main force by fighting to gain time, whilst also observing and reporting information.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Serving as a rear guard</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SRVRSF">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Operating as a force that may be committed into combat only on the order of the commander of the organisation who so designated the reserve force.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Serving as a reserve</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SRVADV">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Operating as a security element whose primary task is to move ahead of the main body and protect the main force by fighting to gain time, whilst also observing and reporting information.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Serving as an advance guard</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SRVINP">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Operating as a force that provides fire and other support to the bridgehead force during an obstacle crossing.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Serving as an in-place force</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="STNGUP">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Establishing a FACILITY, ORGANISATION or FEATURE.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Setting up</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SHOTNG">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The act of firing of a weapon, usually at a person.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Shooting</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SNPATK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An attack by one who shoots at others from a concealed place.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Sniper attack</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SPACAC">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The unforeseen loss, destruction or damage of a spacecraft.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Space accident</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SPYING">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Obtaining intelligence information furtively as an agent of a foreign power or competition.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Spying</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="STAWAR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A state characterised by hostile military activity between the parts.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">State of war</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="STRFAR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Attack by enemy aircraft against ground targets using forward firing ordnance (bullets, shells, or rockets).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Strafing, aerial</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="STRIKE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The organised refusal by employees to work until some grievance is remedied.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Strike</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SUICDE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The action of killing oneself intentionally.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Suicide</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SUPRTN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Aiding, protecting, complementing or sustaining an object.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Supporting</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SUPRSN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Providing fires that neutralises or temporarily degrades the capabilities of enemy forces within a specific area. This makes no assumptions as to enemy casualties; it may be a transitory effect.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Suppressing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SURREN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Yielding to the control or power of the enemy.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Surrender</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SURVEL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The systematic observation of aerospace, surface or subsurface areas, places, persons, or things, by electronic means.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Surveillance, electronic</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SUSHOS">
