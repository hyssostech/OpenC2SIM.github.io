	<xs:simpleType name="MaterielTypeSupplyClassCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that represents the NATO supply class of MATERIEL-TYPE.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			
			<xs:enumeration value="CLS1">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Items that are consumed by personnel and animals at an approximately uniform rate, irrespective of local changes in combat or terrain conditions, e.g. food and forage.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Class I</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CLS2">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Supplies for which allowances are established by tables of organisation and equipment, e.g. clothing, weapons, tools, spare parts, vehicles.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Class II</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CLS3">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Fuel and lubricants for all purposes, except for operating aircraft or for use in weapons such as flame-throwers, e.g. gasoline, fuel oil, greases, coal and coke.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Class III</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CLS3A">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Aviation fuel and lubricants.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Class IIIa</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CLS4">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Supplies for which initial use allowances are not prescribed by approved issue tables. Normally includes fortification and construction materials, as well as additional quantities of items identical to those authorised for initial issue (Class II) such as additional vehicles.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Class IV</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CLS5">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Ammunition, explosives and chemical agents of all types.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Class V</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="MedicalFacilityStatusIntervalCasualtyTypeCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that represents the categorisation of casualties according to the type of illness or injury in a specific MEDICAL-FACILITY-STATUS-INTERVAL-CASUALTY-TYPE.</xs:documentation>
		</xs:annotation>
