	<xs:simpleType name="CandidateTargetDetailFocusTypeCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that denotes a general class of actions intended by the nominating authority for a specific CANDIDATE-TARGET-DETAIL.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ATTACK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To conduct a type of offensive action characterised by coordinated employment of firepower and manoeuvre to close with and destroy or capture the enemy.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Attack</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CAPTUR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To take possession of an object, normally by force; it frequently involves movement as a preliminary phase.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Capture</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DEFEAT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To diminish the effectiveness of the enemy to the extent that he is unable to participate further in the battle or at least cannot fulfil his intention.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Defeat</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DESTRY">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To physically render a target combat-ineffective or damage it so that it cannot function as intended nor be restored to a usable condition without being entirely rebuilt.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Destroy</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DONOTA">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specific object or type in the CANDIDATE-TARGET-DETAIL must not be attacked.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Do not attack</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ILLUMN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Battlespace illumination provided by employing searchlight or pyrotechnic illuminants using diffusion or reflection.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Illuminate</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INFILT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To move a force, broken down as individuals or small groups, over, through or around enemy positions with the aim of avoiding detection.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Infiltrate</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INTCEP">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To conduct Electronic Warfare Support operations with a view to searching, locating, recording and analysing radiated electromagnetic energy for the purposes of supporting an operation.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Intercept</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="JAM">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To deliberately radiate, re-radiate or reflect electromagnetic energy with the object of impairing the use of electronic devices, equipment or systems being used by the enemy.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Jam</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LOCATE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To establish the position of an objective.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Locate</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MARK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To make visible (by the use of light/IR/laser/arty) an objective in order to allow its identification by another object (usually as a precursor to the use of direct fire weapons against the marked objective).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Mark</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NUTRLS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To render the enemy’s weapons temporarily ineffective, normally by use of indirect fire (also normally associated with the imposition of 10% casualties in the case of indirect fire).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Neutralise</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NOS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The appropriate value is not in the set of specified values.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Not otherwise specified</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OBSRV">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To provide continuous view, and the potential for reports on the activity of an objective.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Observe</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OCCUPY">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">To move into and properly organise an area to be used as a battle position.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Occupy</DisplayValue>
