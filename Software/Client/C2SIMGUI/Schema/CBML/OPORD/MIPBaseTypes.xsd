					<xs:appinfo>
						<DisplayValue xml:lang="en">In progress</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NOOPER">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The planned task was not carried out during a specified period.</Definition>
