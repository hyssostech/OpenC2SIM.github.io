<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	       xmlns:jc3iedm="urn:int:nato:standard:mip:jc3iedm:3.0.2:oo:2.2" 
	       xmlns:cbml="http://www.sisostds.org/schemas/c-bml/1.0" 
	       targetNamespace="http://www.sisostds.org/schemas/c-bml/1.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:annotation>
		<xs:documentation xml:lang="en">Code Value Enumerations taken from US-JC3IEDM-v3_1b-BaseTypes.xsd</xs:documentation>
	</xs:annotation>

	<xs:simpleType name="ActionContextCategoryCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that represents the nature of the ACTION-CONTEXT as it relates to a specific ACTION and a specific CONTEXT.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="DES">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An expected positive consequence of a specific CONTEXT in relation to a specific ACTION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Desired</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FINACT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An actual specific CONTEXT as it exists at the end of a specific ACTION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Final state, actual</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FINPLA">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A planned specific CONTEXT that may exist at the end of a specific ACTION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Final state, planning</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INIACT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An actual specific CONTEXT as it exists at the start of a specific ACTION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Initial state, actual</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INIPLA">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A planned specific CONTEXT that may exist at the start of a specific ACTION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Initial state, planning</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INTACT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An actual specific CONTEXT as it exists at an intermediate point during the execution of a specific ACTION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Intermediate state, actual</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INTPLA">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A planned specific CONTEXT that may exist at an intermediate point during the execution of a specific ACTION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Intermediate state, planning</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ISINCL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The specific ACTION is encompassed within the information content of a specific CONTEXT.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Is included in</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MAX">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The maximum value referred to by a specific CONTEXT that is necessary for the evolution of a specific ACTION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Maximum required</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MIN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The minimum value referred to by a specific CONTEXT that is necessary for the evolution of a specific ACTION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Minimum required</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ActionEffectDescriptionCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that represents the type of outcome of a specific ACTION that is being estimated or recorded.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="BURN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The ACTION objective is alight.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Burning</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CAPTRD">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The ACTION objective has been captured or acquired and is available for use or interrogation/possession has been taken of an area, normally by force.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Captured</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CONS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The consumption of specified consumables or CONSUMABLE-MATERIAL-TYPEs.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Consumed</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DSTRYK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Cannot function as intended nor be repaired or restored to an operational status.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Destroyed (K-kill)</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FKIL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The destruction of a vehicle (or system's) primary weapon system.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Firepower kill (F-kill)</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FLIG">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The setting to flight of a PERSON or PERSON-TYPEs from their present location.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Forced to flee</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="IDNT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The ACTION objective has been either identified (i.e., classified as friend/foe/neutral) or recognised (i.e., the unit designation of the ORGANISATION or object is known).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Identified</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ILLUMN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The temporary supply of IR or white-light illumination (usually in coordination with direct or indirect fire).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Illuminated</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="INTREC">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An equipment lost in an area unsuitable for recovery due to political, military or geographic/environmental considerations.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Intact but unrecoverable</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="KILL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A casualty who is killed outright or dies as a result of wounds, disease or other injuries.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Killed</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LDAM">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The subjective categorisation of the physical effect of an ACTION on buildings, infrastructure and/or equipment by a reporting ORGANISATION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Light damage</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LGTRST">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The ACTION-OBJECTIVE reacted against the ACTION using light physical violence.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Light resistance</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LOST">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The object is no longer available for military operations.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Lost</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MKIL">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The rendering of a vehicle as being temporarily or permanently incapable of tactical movement.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Mobility kill (M-kill)</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MODDAM">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The subjective categorisation of the physical effect of an ACTION on buildings, infrastructure and/or equipment by a reporting ORGANISATION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Moderate damage</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NUTRLD">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The rendering of the ACTION objective temporarily ineffective by the infliction of significant casualties (norm for indirect fire is 10 percent).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Neutralised</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NORSTN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The ACTION-OBJECTIVE did not react against the ACTION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">No resistance</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NBCAS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A person who is not a battle casualty, but who is lost to his organisation by reason of disease or injury, including persons dying from disease or injury, or by reason of being missing where the absence does not appear to be voluntary or due to enemy action or to being interned.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Non-battle casualty</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NKN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">It is not possible to determine which value is most applicable.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Not known</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NOS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The appropriate value is not in the set of specified values.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Not otherwise specified</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SDAM">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The subjective categorisation of the physical effect of an ACTION on buildings, infrastructure and/or equipment by a reporting ORGANISATION.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Severe damage</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SUPRSD">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Reduction of the effectiveness of the ACTION objective for a specific period or purpose (normally to prevent an enemy from interfering with friendly ACTIONs); this effect may be entirely transitory and cause no casualties.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Suppressed</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VRBPRT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The ACTION-OBJECTIVE reacted against the ACTION using verbal abuse.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Verbal protest</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VLNRST">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The ACTION-OBJECTIVE reacted against the ACTION using heavy physical violence.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Violent resistance</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WNDD">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A casualty other than "killed" who has incurred an injury due to an external agent or cause.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Wounded</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ActionEffectSeverityCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that represents the degree of seriousness of a specific ACTION-EFFECT.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="MINDSR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A subjective judgement by the reporting organisation that the specific ACTION resulted in minor disorder with respect to the object or class that is the focus of the ACTION-EFFECT.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Minor disruption</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NODSRP">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A subjective judgement by the reporting organisation that the specific ACTION resulted in no disorder with respect to the object or class that is the focus of the ACTION-EFFECT.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">No disruption</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NKN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">It is not possible to determine which value is most applicable.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Not known</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SEVDSR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A subjective judgement by the reporting organisation that the specific ACTION resulted in severe disorder with respect to the object or class that is the focus of the ACTION-EFFECT.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Severe disruption</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TOTDSR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">A subjective judgement by the reporting organisation that the specific ACTION resulted in complete disorder with respect to the object or class that is the focus of the ACTION-EFFECT.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Total disruption</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="ActionEventCategoryCodeXmlType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specific value that represents the general class or nature of activity prescribed by an ACTION-EVENT.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ABDICA">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The renouncement by a person of his official functions.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Abdication</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ACCDNT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An unfortunate event, esp. one causing physical harm or damage, brought about unintentionally.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Accident</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ACCAIR">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An accident involving an aircraft during ground operations or maintenance.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Accident, aircraft ground</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ACCMNE">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An accident involving unexploded ordnance.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Accident, mine</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ACCTRF">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An accident involving at least one motor vehicle.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Accident, traffic</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ACCWPN">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An accident involving a weapon.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Accident, weapon</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ACCWRK">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">An accident occurring at the workplace.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Accident, workplace</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ADVANC">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Moving towards an objective in some form of tactical formation. This is a transitional phase between operations that may or may not result in contact with the enemy.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Advancing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AERENG">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The occurrence of a hostile encounter between military aircraft.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Aerial engagement</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AERSHO">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The deliberate destruction of an aircraft.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Aerial shoot down</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ARASLT">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Mounting an assault utilising a mixture of aviation and ground transport, the principal feature of which is the insertion of combat power.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Air assault</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ARBNAS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">Mounting an airborne operation, a phase beginning with delivery by air of the assault echelon of the force into the objective area and extending through attack of assault objectives and consolidation of the initial airhead.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Airborne assault</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AIRCRS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The unforeseen destruction or damage of an aircraft (and personnel).</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Aircraft crash</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AIRLND">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The action of approaching and alighting on the ground or some other surface after a flight.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Aircraft landing</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="AIRLAU">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The occurrence of one or more aircraft taking off.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Aircraft launch activity</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="ACRLOS">
				<xs:annotation>
					<xs:documentation>
						<Definition xml:lang="en">The unforeseen loss of an aircraft (and personnel), other than crash.</Definition>
					</xs:documentation>
					<xs:appinfo>
						<DisplayValue xml:lang="en">Aircraft loss</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
