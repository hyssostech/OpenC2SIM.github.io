						<DisplayValue xml:lang="en">Acceptable</DisplayValue>
					</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="MARGNL">
				<xs:annotation>
					<xs:documentation>
