<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jc3iedm="urn:int:nato:standard:mip:jc3iedm:3.0.2:oo:2.2" xmlns:cbml="http://www.sisostds.org/schemas/c-bml/1.0" xmlns:xml="http://www.w3.org/XML/1998/namespace" targetNamespace="http://www.sisostds.org/schemas/c-bml/1.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:annotation>
		<xs:documentation xml:lang="en">Coalition Battle Management Language (C-BML) information components defined for the Simulation Interoperability Standards Organization (SISO) C-BML Phase 1 Specification (Saikou Diallo VMASC sdiallo@odu.edu, Marc St-Onge Canadian Signals Inc, Sidney Chartrand CAE Inc, Curt Blais NPS Co-edtiors, C-BML Drafting Group). References the Joint Command, Control, and Consultation Information Exchange Data Model (JC3IEDM) XML schema defined by the Multilateral Interoperability Programme (MIP) - Editor: Dr. Michael Gerz, gerz@fgan.de, FGAN FKIE, Germany - Tue Dec 06 18:35:09 CET 2005 - </xs:documentation>
	</xs:annotation>
	<xs:import namespace="urn:int:nato:standard:mip:jc3iedm:3.0.2:oo:2.2" schemaLocation="../../jc3iedm-simple-types/3.0.2/jc3iedm-simple-types.xsd"/>
	<xs:include schemaLocation="../../cbml-entity-types/1.0/cbml-entity-types.xsd"/>
	<!-- ********************************************************** Detailed W's **************************************************************-->
	<!-- ************* Who Elements *************-->
	<xs:element name="Who" type="cbml:WhoType"/>
	<xs:element name="WhoRef" type="cbml:WhoRefType"/>
	<xs:element name="ExecuterWho" type="cbml:ExecuterWhoType"/>
	<xs:element name="TaskerWho" type="cbml:TaskerWhoType"/>
	<xs:element name="TaskeeWho" type="cbml:TaskeeWhoType"/>
	<xs:element name="ReporterWho" type="cbml:ReporterWhoType"/>
	<xs:element name="RequesterWho" type="cbml:RequesterWhoType"/>
	<xs:element name="RequestedWho" type="cbml:RequestedWhoType"/>
	<xs:element name="AffectedWho" type="cbml:AffectedWhoType"/>
	<!-- ************** What Elements ********** -->
	<xs:element name="What" type="cbml:WhatType"/>
	<xs:element name="WhatRef" type="cbml:WhatRefType"/>
	<xs:element name="TaskWhat" type="cbml:TaskWhatType"/>
	<xs:element name="TaskWhatRef" type="cbml:TaskWhatRefType"/>
	<xs:element name="EventWhat" type="cbml:EventWhatType"/>
	<xs:element name="EventWhatRef" type="cbml:EventWhatRefType"/>
	<!-- ************** When Elements ********** -->
	<xs:element name="TaskWhen" type="cbml:TaskWhenType"/>
	<xs:element name="EventStartWhen" type="cbml:EventStartWhenType"/>
	<xs:element name="EventEndWhen" type="cbml:EventEndWhenType"/>
	<xs:element name="ReportedWhen" type="cbml:AbstractReportedWhenType"/>
	<!-- ************* Where Elements *************-->
	<xs:element name="Where" type="cbml:WhereType"/>
	<xs:element name="RouteWhere" type="cbml:RouteWhereType"/>
	<!-- ************* Why Elements *************-->
	<xs:element name="Why" type="cbml:WhyType"/>
	<!-- ************* Who Types *************-->
	<xs:complexType name="WhoType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies a who.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ObjectItem" type="cbml:AbstractObjectItem">
				<xs:annotation>
					<xs:documentation xml:lang="en">An individually identified object that has military or civilian significance. Concrete types are: {Atmosphere, CloudCover, Icing, Light, OtherMeteorologicFeature, Precipitation, Visibility, Wind, AirRouteSegment, OtherRouteSegment, AirspaceControlMeans, OtherControlFeature, Route, GeographicFeature, OtherFeature, InstrumentLandingSystem, OtherMateriel, MinefieldLand, MinefieldMaritime, OtherMilitaryObstacle, Airfield, Anchorage, Apron, Basin, Berth, Bridge, DryDock, Harbour, Jetty, OtherFacility, Quay, Railway, Road, Slipway, Convoy, OtherOrganisation, Unit, OtherObjectItem, Person}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WhoRefType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies a reference on a  who.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ObjectItemRef" type="cbml:AbstractObjectItemRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">A reference to some ObjectItem - An individually identified object that has military or civilian significance. Concrete types are: {AtmosphereRef, CloudCoverRef, IcingRef, LightRef, OtherMeteorologicFeatureRef, PrecipitationRef, VisibilityRef, WindRef, AirRouteSegmentRef, OtherRouteSegmentRef, AirspaceControlMeansRef, OtherControlFeatureRef, RouteRef, GeographicFeatureRef, OtherFeatureRef, InstrumentLandingSystemRef, OtherMaterielRef, MinefieldLandRef, MinefieldMaritimeRef, OtherMilitaryObstacleRef, AirfieldRef, AnchorageRef, ApronRef, BasinRef, BerthRef, BridgeRef, DryDockRef, HarbourRef, JettyRef, OtherFacilityRef, QuayRef, RailwayRef, RoadRef, SlipwayRef, ConvoyRef, OtherOrganisationRef, UnitRef, OtherObjectItemRef, PersonRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ExecuterWhoType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the who that is reported be executing an event by a ReporterWho. Note that in the case of an organization reporting its own unplanned activity (Event), the ExecuterWho and the ReporterWho are the same organization.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganisationRef" type="cbml:AbstractOrganisationRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">A reference to some Organisation - An ObjectItem that is an administrative or functional structure. Concrete types are: {ConvoyRef, OtherOrganisationRef, UnitRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaskerWhoType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies who is ordering or authorizing execution of a task.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganisationRef" type="cbml:AbstractOrganisationRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">A reference to some Organisation - An ObjectItem that is an administrative or functional structure. Concrete types are: {ConvoyRef, OtherOrganisationRef, UnitRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaskeeWhoType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies who is executing a task.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganisationRef" type="cbml:AbstractOrganisationRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">A reference to some Organisation - An ObjectItem that is an administrative or functional structure. Concrete types are: {ConvoyRef, OtherOrganisationRef, UnitRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ReporterWhoType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies who is reporting.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganisationRef" type="cbml:AbstractOrganisationRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">A reference to some Organisation - An ObjectItem that is an administrative or functional structure. Concrete types are: {ConvoyRef, OtherOrganisationRef, UnitRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RequesterWhoType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies who is making a request for a task.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganisationRef" type="cbml:AbstractOrganisationRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">A reference to some Organisation - An ObjectItem that is an administrative or functional structure. Concrete types are: {ConvoyRef, OtherOrganisationRef, UnitRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RequestedWhoType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies who is being requested to perform a task.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OrganisationRef" type="cbml:AbstractOrganisationRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">A reference to some Organisation - An ObjectItem that is an administrative or functional structure. Concrete types are: {ConvoyRef, OtherOrganisationRef, UnitRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AffectedWhoType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies who is affected by the execution of an action (task or event).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ActionObjective" type="cbml:AbstractActionObjective">
				<xs:annotation>
					<xs:documentation xml:lang="en">The focus, in terms of an ObjectItem or ObjectType, in conducting a specific task or event. Concrete types are: {OtherActionObjectiveType, OtherActionObjectiveItem, Target}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ************** What Types ********** -->
	<xs:complexType name="WhatType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies an action.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Action" type="cbml:AbstractAction">
				<xs:annotation>
					<xs:documentation xml:lang="en">A task or event that may utilise resources and may be focused against an objective. Concrete types are: {NuclearWeaponEvent, OtherNuclearEvent, OtherRadioactiveEvent, RadiologicalEvent, ChemicalBiologicalEvent, OtherCbrnEvent, OtherActionEvent, OtherActionTask, Request}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WhatRefType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies a reference to an action.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ActionRef" type="cbml:AbstractActionRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">A reference to some Action - A task or event that may utilise resources and may be focused against an objective. Concrete types are: {NuclearWeaponEventRef, OtherNuclearEventRef, OtherRadioactiveEventRef, RadiologicalEventRef, ChemicalBiologicalEventRef, OtherCbrnEventRef, OtherActionEventRef, OtherActionTaskRef, RequestRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaskWhatType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies a task.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ActionTask" type="cbml:AbstractActionTask">
				<xs:annotation>
					<xs:documentation xml:lang="en">An task that is being or has been planned and for which the planning details are known. Concrete types are: {OtherActionTask, Request}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaskWhatRefType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies a reference to task.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ActionTaskRef" type="cbml:AbstractActionTaskRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">A reference to some ActionTask - An task that is being or has been planned and for which the planning details are known. Concrete types are: {OtherActionTaskRef, RequestRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EventWhatType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies an event.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ActionEvent" type="cbml:AbstractActionEvent">
				<xs:annotation>
					<xs:documentation xml:lang="en">An event that is an incident, phenomenon, or occasion of military significance which has occurred or is occurring but for which planning is not known. Concrete types are: {NuclearWeaponEvent, OtherNuclearEvent, OtherRadioactiveEvent, RadiologicalEvent, ChemicalBiologicalEvent, OtherCbrnEvent, OtherActionEvent}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EventWhatRefType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies a reference to an event.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ActionEventRef" type="cbml:AbstractActionEventRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">A reference to some ActionEvent - An event that is an incident, phenomenon, or occasion of military significance which has occurred or is occurring but for which planning is not known. Concrete types are: {NuclearWeaponEventRef, OtherNuclearEventRef, OtherRadioactiveEventRef, RadiologicalEventRef, ChemicalBiologicalEventRef, OtherCbrnEventRef, OtherActionEventRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ************* When Types *************-->
	<xs:complexType name="TaskStartWhenType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the start time of a task.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="StartAbsoluteTime" type="cbml:StartWhenAbsoluteTimeType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Specifies the start time of a task in absolute time.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartRelativeTime" type="cbml:StartWhenRelativeTimeType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Specifies the start time of a task in relative time.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="EventStartWhenType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the start time of an event.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="StartRelativeTime" type="cbml:StartWhenRelativeTimeType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Specifies the start time of an event in relative time.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TaskEndWhenType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the end time of a task.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="EndAbsoluteTime" type="cbml:EndWhenAbsoluteTimeType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Specifies the end time of a task in absolute time.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndRelativeTime" type="cbml:EndWhenRelativeTimeType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Specifies the end time of a task in relative time.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="EventEndWhenType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the end time of an event.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="EndRelativeTime" type="cbml:EndWhenRelativeTimeType">
				<xs:annotation>
					<xs:documentation xml:lang="en">Specifies the end time of an event in relative time.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="StartWhenAbsoluteTimeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the start time of a task in absolute time.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TimingDayCode" type="jc3iedm:ActionTaskTimingDayCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents the notional start of the task in terms of a day with defined operational meaning.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TimingHourCode" type="jc3iedm:ActionTaskTimingHourCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents the notional start of the task in terms of an hour with defined operational meaning.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="StartSpecifiedTime" type="cbml:StartWhenAbsoluteSpecifiedTimeType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Specifies the start time of a task in absolute time.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="StartUnspecifiedTime" type="cbml:StartWhenAbsoluteUnspecifiedTimeType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Specifies the start time of a task as a category code.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StartWhenAbsoluteSpecifiedTimeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the start time of a task in absolute time.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Datetime" type="jc3iedm:DatetimeTypeFix18">
				<xs:annotation>
					<xs:documentation xml:lang="en">The character string representing a point in time that designates the occurrence of the planned beginning of the specific task</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartTimeQualifierCode" type="cbml:ActionTaskStartTimeQualifierCode">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that denotes the role of starting date and time with respect to the period of effectiveness of a specific task.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StartWhenAbsoluteUnspecifiedTimeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the start time of a task as a category code.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StartQualifierCode" type="cbml:ActionTaskStartQualifierCode">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that denotes the role of starting date and time with respect to the period of effectiveness of a specific task.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EndWhenAbsoluteTimeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the end time of a task in absolute time.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:element name="EndSpecifiedTime" type="cbml:EndWhenAbsoluteSpecifiedTimeType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Specifies the end time of a task in absolute time.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EndUnspecifiedTime" type="cbml:EndWhenAbsoluteUnspecifiedTimeType">
					<xs:annotation>
						<xs:documentation xml:lang="en">Specifies the end time of a task as a qualifier code.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EndWhenAbsoluteSpecifiedTimeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the end time of a task in absolute time.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Datetime" type="jc3iedm:DatetimeTypeFix18">
				<xs:annotation>
					<xs:documentation xml:lang="en">The character string representing a point in time that designates the occurrence of the planned conclusion of the specific task.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndTimeQualifierCode" type="cbml:ActionTaskEndTimeQualifierCode">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that denotes the role of ending date and time with respect to the period of effectiveness of a specific task.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EndWhenAbsoluteUnspecifiedTimeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the end time of a task as a qualifier code.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EndQualifierCode" type="cbml:ActionTaskEndQualifierCode">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that denotes the role of ending date and time with respect to the period of effectiveness of a specific task.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StartWhenRelativeTimeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the start time of an action (task or event) in relative time.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ObjectActionRef" type="cbml:AbstractActionRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">ObjectActionRef is the child in a 'is-the-subject-of' relationship. Concrete types are: {NuclearWeaponEventRef, OtherNuclearEventRef, OtherRadioactiveEventRef, RadiologicalEventRef, ChemicalBiologicalEventRef, OtherCbrnEventRef, OtherActionEventRef, ActionTaskRef, RequestRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StartTemporalAssociationCategoryCode" type="cbml:ActionStartTemporalAssociationCategoryCode">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents the class of chronological relationship of a subject action (task or event) to an object action (task or event).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReferenceDuration" type="jc3iedm:DurationType19" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The numeric value that represents a quantity of time in milliseconds that elapses after the start or end of a specific object action (task or event) that a subject action (task or event) is referenced to for its execution.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EndWhenRelativeTimeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the end time of an action (task or event) in relative time. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ObjectActionRef" type="cbml:AbstractActionRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">ObjectActionRef is the child in a 'is-the-subject-of' relationship. Concrete types are: {NuclearWeaponEventRef, OtherNuclearEventRef, OtherRadioactiveEventRef, RadiologicalEventRef, ChemicalBiologicalEventRef, OtherCbrnEventRef, OtherActionEventRef, ActionTaskRef, RequestRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndTemporalAssociationCategoryCode" type="cbml:ActionEndTemporalAssociationCategoryCode">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents the class of chronological relationship of a subject action (task or event) to an object action (task or event).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReferenceDuration" type="jc3iedm:DurationType19" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The numeric value that represents a quantity of time in milliseconds that elapses after the start or end of a specific object action (task or event) that a subject action (task or event) is referenced to for its execution.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaskWhenType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Specifies the duration and start/end time of a task.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MinimumDuration" type="jc3iedm:DurationType19" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The numeric value that represents a quantity of time in milliseconds for the minimum permissible period of effectiveness of a specific task.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EstimatedDuration" type="jc3iedm:DurationType19" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The numeric value that represents a quantity of time in milliseconds for the estimated period of effectiveness of a specific task.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MaximumDuration" type="jc3iedm:DurationType19" minOccurs="0">
				<xs:annotation>
