<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jc3iedm="urn:int:nato:standard:mip:jc3iedm:3.0.2:oo:2.2" xmlns:cbml="http://www.sisostds.org/schemas/c-bml/1.0" targetNamespace="http://www.sisostds.org/schemas/c-bml/1.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:annotation>
		<xs:documentation xml:lang="en">XML schema defined by the Multilateral Interoperability Programme (MIP) - Editor: Dr. Michael Gerz, gerz@fgan.de, FGAN FKIE, Germany</xs:documentation>
		<xs:documentation xml:lang="en">The following schema is directly derived from the JC3IEDM 3.0.2:oo:2.2 schema.</xs:documentation>
	</xs:annotation>
	<xs:import namespace="urn:int:nato:standard:mip:jc3iedm:3.0.2:oo:2.2" schemaLocation="../../jc3iedm-simple-types/3.0.2/jc3iedm-simple-types.xsd"/>
	<xs:include schemaLocation="../../cbml-entity-types/1.0/cbml-entity-types.xsd"/>
	<!-- Concrete Types Definition -->
	<xs:complexType name="CorridorArea">
		<xs:annotation>
			<xs:documentation xml:lang="en">A SURFACE that is defined by its width and a sequence of points.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractSurface">
				<xs:sequence>
					<xs:element name="WidthDimension" type="jc3iedm:DimensionType12_3">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the horizontal distance measured from side to side for a CORRIDOR-AREA and distributed equally with respect to its centreline.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice>
						<xs:element name="CentreLine" type="cbml:Line">
							<xs:annotation>
								<xs:documentation xml:lang="en">The LINE that defines the central line for a specific CORRIDOR-AREA.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CentreLineRef" type="cbml:LineRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The LINE that defines the central line for a specific CORRIDOR-AREA.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CorridorAreaRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some CorridorArea - A SURFACE that is defined by its width and a sequence of points.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractSurfaceRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Line">
		<xs:annotation>
			<xs:documentation xml:lang="en">A LOCATION that is defined by two or more POINTs connected by one-dimensional line segments in an ordered sequence.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractLocation">
				<xs:sequence>
					<xs:choice minOccurs="2" maxOccurs="unbounded">
						<xs:element name="Point" type="cbml:AbstractPoint">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT that is used as a Line's point in specific sequence in defining a particular LINE. Concrete types are: {CartesianPoint, GeographicPoint, RelativePoint}</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PointRef" type="cbml:AbstractPointRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">A reference to some Point. - The POINT that is used as a Line's point in specific sequence in defining a particular LINE. Concrete types are: {CartesianPointRef, GeographicPointRef, RelativePointRef}</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="LineRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Line - A LOCATION that is defined by two or more POINTs connected by one-dimensional line segments in an ordered sequence.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractLocationRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GeographicPoint">
		<xs:annotation>
			<xs:documentation xml:lang="en">An ABSOLUTE-POINT that has its position specified with respect to the surface of the World Geodetic System 1984 (WGS 84) ellipsoid.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractAbsolutePoint">
				<xs:sequence>
					<xs:element name="LatitudeCoordinate" type="jc3iedm:LatitudeCoordinateTypeRangeLatitude9_6">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the angle between the plane of the equator and a line perpendicular to the ellipsoid surface and passing through the GEOGRAPHIC-POINT.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LongitudeCoordinate" type="jc3iedm:LongitudeCoordinateTypeRangeLongitude10_6">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the angle between the initial (zero or Greenwich) meridian and the meridian of the GEOGRAPHIC-POINT measured in the plane of the Equator.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LatitudePrecisionCode" type="jc3iedm:AnglePrecisionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the resolution used for the expression of a value of a latitude coordinate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LongitudePrecisionCode" type="jc3iedm:AnglePrecisionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the resolution used for the expression of a value of a longitude coordinate.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GeographicPointRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some GeographicPoint - An ABSOLUTE-POINT that has its position specified with respect to the surface of the World Geodetic System 1984 (WGS 84) ellipsoid.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractAbsolutePointRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PolygonArea">
		<xs:annotation>
			<xs:documentation xml:lang="en">A SURFACE that has its boundaries defined by a specific LINE.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractSurface">
				<xs:sequence>
					<xs:choice>
						<xs:element name="BoundingLine" type="cbml:Line">
							<xs:annotation>
								<xs:documentation xml:lang="en">The closed LINE that constitutes the boundary for a specific POLYGON-AREA.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="BoundingLineRef" type="cbml:LineRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The closed LINE that constitutes the boundary for a specific POLYGON-AREA.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PolygonAreaRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some PolygonArea - A SURFACE that has its boundaries defined by a specific LINE.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractSurfaceRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SurfaceVolume">
		<xs:annotation>
			<xs:documentation xml:lang="en">A GEOMETRIC-VOLUME that has its horizontal boundaries defined by a specific SURFACE.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractGeometricVolume">
				<xs:sequence>
					<xs:choice>
						<xs:element name="DefiningSurface" type="cbml:AbstractSurface">
							<xs:annotation>
								<xs:documentation xml:lang="en">The SURFACE that defines the horizontal boundaries for a specific SURFACE-VOLUME. Concrete types are: {CorridorArea, Ellipse, FanArea, OrbitArea, PolyarcArea, PolygonArea, TrackArea}</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DefiningSurfaceRef" type="cbml:AbstractSurfaceRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The SURFACE that defines the horizontal boundaries for a specific SURFACE-VOLUME. Concrete types are: {CorridorAreaRef, EllipseRef, FanAreaRef, OrbitAreaRef, PolyarcAreaRef, PolygonAreaRef, TrackAreaRef}</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SurfaceVolumeRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some SurfaceVolume - A GEOMETRIC-VOLUME that has its horizontal boundaries defined by a specific SURFACE.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractGeometricVolumeRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OtherLocation">
		<xs:annotation>
			<xs:documentation xml:lang="en">A specification of position and geometry with respect to a specified horizontal frame of reference and a vertical distance measured from a specified datum.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractLocation">
				<xs:sequence>
					<xs:element name="LocationCategoryCode" type="jc3iedm:LocationCategoryCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents a class of LOCATION. It serves as a discriminator that partitions LOCATION into subtypes.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OtherLocationRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some OtherLocation - A specification of position and geometry with respect to a specified horizontal frame of reference and a vertical distance measured from a specified datum.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractLocationRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CartesianPoint">
		<xs:annotation>
			<xs:documentation xml:lang="en">An ABSOLUTE-POINT that has its position specified in a three-dimensional Earth-centred Cartesian system.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractAbsolutePoint">
				<xs:sequence>
					<xs:element name="XCoordinateDimension" type="jc3iedm:DimensionType12_3">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the X-component of a coordinate which expresses the position of a point in a three-dimensional Cartesian coordinate system that is fixed to the earth, where the X-axis lies in the planes of the Equator and the Greenwich meridian.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="YCoordinateDimension" type="jc3iedm:DimensionType12_3">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the Y-component of a coordinate which expresses the position of a point in a three-dimensional Cartesian coordinate system that is fixed to the earth where the Y-axis is perpendicular to both the X- and Z-axes completing the right-handed coordinate system.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ZCoordinateDimension" type="jc3iedm:DimensionType12_3">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the Z-component of a coordinate which expresses the position of a point in a three-dimensional Cartesian coordinate system that is fixed to the earth, where the Z-axis coincides with the mean rotation axis of the Earth.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="XPrecisionCode" type="jc3iedm:DistancePrecisionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the resolution used for the expression of a value of a Cartesian x-coordinate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="YPrecisionCode" type="jc3iedm:DistancePrecisionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the resolution used for the expression of a value of a Cartesian y-coordinate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ZPrecisionCode" type="jc3iedm:DistancePrecisionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the resolution used for the expression of a value of a Cartesian z-coordinate.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CartesianPointRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some CartesianPoint - An ABSOLUTE-POINT that has its position specified in a three-dimensional Earth-centred Cartesian system.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractAbsolutePointRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ConeVolume">
		<xs:annotation>
			<xs:documentation xml:lang="en">A GEOMETRIC-VOLUME whose boundary is swept by a line that has a fixed point and another that moves along the path defined by the border of a specific SURFACE.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractGeometricVolume">
				<xs:sequence>
					<xs:choice>
						<xs:element name="DefiningSurface" type="cbml:AbstractSurface">
							<xs:annotation>
								<xs:documentation xml:lang="en">The SURFACE that is the defining cross-section for a specific CONE-VOLUME. Concrete types are: {CorridorArea, Ellipse, FanArea, OrbitArea, PolyarcArea, PolygonArea, TrackArea}</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DefiningSurfaceRef" type="cbml:AbstractSurfaceRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The SURFACE that is the defining cross-section for a specific CONE-VOLUME. Concrete types are: {CorridorAreaRef, EllipseRef, FanAreaRef, OrbitAreaRef, PolyarcAreaRef, PolygonAreaRef, TrackAreaRef}</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:choice>
						<xs:element name="VertexPoint" type="cbml:AbstractPoint">
							<xs:annotation>
								<xs:documentation xml:lang="en">The specific POINT that is the vertex of a specific CONE-VOLUME. Concrete types are: {CartesianPoint, GeographicPoint, RelativePoint}</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="VertexPointRef" type="cbml:AbstractPointRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The specific POINT that is the vertex of a specific CONE-VOLUME. Concrete types are: {CartesianPointRef, GeographicPointRef, RelativePointRef}</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ConeVolumeRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some ConeVolume - A GEOMETRIC-VOLUME whose boundary is swept by a line that has a fixed point and another that moves along the path defined by the border of a specific SURFACE.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractGeometricVolumeRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Ellipse">
		<xs:annotation>
			<xs:documentation xml:lang="en">A planar SURFACE in the form of an ellipse.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractSurface">
				<xs:sequence>
					<xs:choice>
						<xs:element name="CentrePoint" type="cbml:AbstractPoint">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT that defines the geometric centre for a specific ELLIPSE. Concrete types are: {CartesianPoint, GeographicPoint, RelativePoint}</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CentrePointRef" type="cbml:AbstractPointRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT that defines the geometric centre for a specific ELLIPSE. Concrete types are: {CartesianPointRef, GeographicPointRef, RelativePointRef}</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:choice>
						<xs:element name="FirstConjugateDiameterPoint" type="cbml:AbstractPoint">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT marking the diameter on the first axis of a specific ELLIPSE. Concrete types are: {CartesianPoint, GeographicPoint, RelativePoint}</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="FirstConjugateDiameterPointRef" type="cbml:AbstractPointRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT marking the diameter on the first axis of a specific ELLIPSE. Concrete types are: {CartesianPointRef, GeographicPointRef, RelativePointRef}</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:choice>
						<xs:element name="SecondConjugateDiameterPoint" type="cbml:AbstractPoint">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT marking the diameter on the second axis of a specific ELLIPSE. Concrete types are: {CartesianPoint, GeographicPoint, RelativePoint}</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SecondConjugateDiameterPointRef" type="cbml:AbstractPointRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT marking the diameter on the second axis of a specific ELLIPSE. Concrete types are: {CartesianPointRef, GeographicPointRef, RelativePointRef}</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EllipseRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Ellipse - A planar SURFACE in the form of an ellipse.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractSurfaceRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FanArea">
		<xs:annotation>
			<xs:documentation xml:lang="en">A SURFACE that is in the form of a truncated ring sector, lying between and being bounded by the rays emanating from the centre-point of the ring and having a specified central angle.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractSurface">
				<xs:sequence>
					<xs:element name="MinimumRangeDimension" type="jc3iedm:DimensionType12_3">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the distance from the vertex to the inner ring of the ring sector used to specify the FAN-AREA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumRangeDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing distance from the vertex to the outer ring of the ring sector used to specify the FAN-AREA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OrientationAngle" type="jc3iedm:AngleTypeRangeAngle7_4">
						<xs:annotation>
							<xs:documentation xml:lang="en">The rotational measurement clockwise between the line of true north and the left side of the sector for a specific FAN-AREA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SectorSizeAngle" type="jc3iedm:AngleTypeRangeAngle7_4">
						<xs:annotation>
							<xs:documentation xml:lang="en">The rotational measurement clockwise between the left and right sides of the sector for a specific FAN-AREA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice>
						<xs:element name="VertexPoint" type="cbml:AbstractPoint">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT that defines the vertex for the specific FAN-AREA. Concrete types are: {CartesianPoint, GeographicPoint, RelativePoint}</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="VertexPointRef" type="cbml:AbstractPointRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT that defines the vertex for the specific FAN-AREA. Concrete types are: {CartesianPointRef, GeographicPointRef, RelativePointRef}</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FanAreaRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some FanArea - A SURFACE that is in the form of a truncated ring sector, lying between and being bounded by the rays emanating from the centre-point of the ring and having a specified central angle.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractSurfaceRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OrbitArea">
		<xs:annotation>
			<xs:documentation xml:lang="en">A SURFACE that is (a) an open rectangular section defined by its width and the distance between the two specific POINTs, (b) is closed by two half-circles with radii equal to half the width, and is positioned left, centred, or right with respect to the line formed by the defining points.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractSurface">
				<xs:sequence>
					<xs:element name="AlignmentCode" type="jc3iedm:OrbitAreaAlignmentCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the placement of a specific ORBIT-AREA with respect to its defining reference axis.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="WidthDimension" type="jc3iedm:DimensionType12_3">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the horizontal distance measured from side to side for a specific ORBIT-AREA.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice>
						<xs:element name="FirstPoint" type="cbml:AbstractPoint">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT that defines the initial point of an axis for a specific ORBIT-AREA. Concrete types are: {CartesianPoint, GeographicPoint, RelativePoint}</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="FirstPointRef" type="cbml:AbstractPointRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT that defines the initial point of an axis for a specific ORBIT-AREA. Concrete types are: {CartesianPointRef, GeographicPointRef, RelativePointRef}</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:choice>
						<xs:element name="SecondPoint" type="cbml:AbstractPoint">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT that defines the final point of an axis for a specific ORBIT-AREA. Concrete types are: {CartesianPoint, GeographicPoint, RelativePoint}</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SecondPointRef" type="cbml:AbstractPointRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The POINT that defines the final point of an axis for a specific ORBIT-AREA. Concrete types are: {CartesianPointRef, GeographicPointRef, RelativePointRef}</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OrbitAreaRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some OrbitArea - A SURFACE that is (a) an open rectangular section defined by its width and the distance between the two specific POINTs, (b) is closed by two half-circles with radii equal to half the width, and is positioned left, centred, or right with respect to the line formed by the defining points.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractSurfaceRef"/>
		</xs:complexContent>
	</xs:complexType>
