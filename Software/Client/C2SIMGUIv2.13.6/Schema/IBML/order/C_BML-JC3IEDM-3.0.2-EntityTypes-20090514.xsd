	<xs:complexType name="AbstractActionResource" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">An OBJECT-ITEM or an OBJECT-TYPE that is required, requested, allocated or otherwise used or planned to be used in conducting a specific ACTION. Concrete types are: {ActionResourceItem, ActionResourceType}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CriticalityIndicatorCode" type="jc3iedm:ActionResourceCriticalityIndicatorCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that denotes a judgement whether a specific resource (OBJECT-ITEM or OBJECT-TYPE) associated with a specific ACTION is essential for the effective completion of that ACTION.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="QualifierCode" type="jc3iedm:ActionResourceQualifierCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents the type of restriction or other qualification applicable to a specific ACTION-RESOURCE for a specific ACTION.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- Authorizing Organisation not required in C-BML
			<xs:choice minOccurs="0">
				<xs:element name="AuthorisingOrganisation" type="C_BML:AbstractOrganisation">
					<xs:annotation>
						<xs:documentation xml:lang="en">The ORGANISATION that authorises the use of a specific ACTION-RESOURCE.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AuthorisingOrganisationRef" type="C_BML:AbstractOrganisationRef">
					<xs:annotation>
						<xs:documentation xml:lang="en">The ORGANISATION that authorises the use of a specific ACTION-RESOURCE.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			-->
			<xs:element name="Employment" type="C_BML:AbstractActionResourceEmployment" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A child in a 'is-used-according-to' relationship.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- Employment not required in C-BML
			<xs:choice minOccurs="0">
				<xs:element name="Employment" type="C_BML:AbstractActionResourceEmployment">
					<xs:annotation>
						<xs:documentation xml:lang="en">A child in a 'is-used-according-to' relationship.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EmploymentRef" type="C_BML:AbstractActionResourceEmploymentRef">
					<xs:annotation>
						<xs:documentation xml:lang="en">A child in a 'is-used-according-to' relationship.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			-->
		</xs:sequence>
	</xs:complexType>
