	<xs:complexType name="Jetty">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY that is a platform that may be fixed or floating extending from a shore, normally attached to a wharf or the shore, and which allows access to a vessel lying alongside, used to secure, protect and provide landing and docking for vessels.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractFacility">
				<xs:sequence>
					<xs:element name="DayLimitNetExplosiveQuantity" type="jc3iedm:QuantityType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum net explosive quantity that can be handled at the JETTY during the day. The unit of measure is kilogram.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumDeadweightTonnageQuantity" type="jc3iedm:QuantityType9" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum deadweight tonnage for a vessel at the specific JETTY. The unit of measure is metric ton.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumDraughtDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the maximum draught of vessel that the specific JETTY can accommodate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NightLimitNetExplosiveQuantity" type="jc3iedm:QuantityType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum net explosive quantity that can be handled at the JETTY during the night. The unit of measure is kilogram.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RailCapacityCount" type="jc3iedm:CountType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value that represents the maximum number of goods/freight cars that the JETTY can accommodate at the same time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RailServedIndicatorCode" type="jc3iedm:JettyRailServedIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the JETTY has railway facilities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VesselMaximumBeamDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the maximum beam or width of vessel that the specific JETTY can accommodate.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="JettyRef">
