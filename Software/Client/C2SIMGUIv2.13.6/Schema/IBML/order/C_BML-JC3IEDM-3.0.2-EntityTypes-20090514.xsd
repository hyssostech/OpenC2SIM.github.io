	<xs:complexType name="AbstractActionTask" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">An ACTION that is being or has been planned and for which the planning details are known. Concrete types are: {ActionTask, Request}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractAction">
				<xs:sequence>
					<xs:element name="ActivityCode" type="jc3iedm:ActionTaskActivityCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the type of activity prescribed by the ACTION-TASK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!-- Timing information moved to Assigned When 
					<xs:element name="MinimumDuration" type="jc3iedm:DurationType19" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents a quantity of time in milliseconds for the minimum permissible period of effectiveness of a specific ACTION-TASK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EstimatedDuration" type="jc3iedm:DurationType19" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents a quantity of time in milliseconds for the estimated period of effectiveness of a specific ACTION-TASK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumDuration" type="jc3iedm:DurationType19" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents a quantity of time in milliseconds for the maximum permissible period of effectiveness of a specific ACTION-TASK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PlannedStartDatetime" type="jc3iedm:DatetimeTypeFix18" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The character string representing a point in time that designates the occurrence of the planned beginning of the specific ACTION-TASK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StartQualifierCode" type="jc3iedm:ActionTaskStartQualifierCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that denotes the role of starting date and time with respect to the period of effectiveness of a specific ACTION-TASK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PlannedEndDatetime" type="jc3iedm:DatetimeTypeFix18" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The character string representing a point in time that designates the occurrence of the planned conclusion of the specific ACTION-TASK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndQualifierCode" type="jc3iedm:ActionTaskEndQualifierCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that denotes the role of ending date and time with respect to the period of effectiveness of a specific ACTION-TASK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					-->
					<xs:element name="PriorityCode" type="jc3iedm:ActionTaskPriorityCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the rank of importance of a specific ACTION-TASK in view of the planning organisation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EntailedSafetyDegreeCode" type="jc3iedm:ActionTaskEntailedSafetyDegreeCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the degree of safety (or risk) entailed with an ordered operation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OvertCovertCode" type="jc3iedm:ActionTaskOvertCovertCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the property of an ACTION-TASK to be overt or covert.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!-- Not Computable
					<xs:element name="DetailText" type="jc3iedm:TextTypeVar255" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The character string assigned to represent the description of an ACTION-TASK’s detail.</xs:documentation>
						</xs:annotation>
					</xs:element>
					-->
					<!-- Moved to Assigned When
					<xs:element name="TimingDayCode" type="jc3iedm:ActionTaskTimingDayCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the notional start of the ACTION in terms of a day with defined operational meaning.</xs:documentation>
						</xs:annotation>
					<xs:element name="TimingHourCode" type="jc3iedm:ActionTaskTimingHourCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the notional start of the ACTION in terms of an hour with defined operational meaning.</xs:documentation>
						</xs:annotation>
					</xs:element>
					-->
					<xs:element name="MeteorologicalImpactCode" type="jc3iedm:ActionTaskMeteorologicalImpactCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents a subjective indication of the effect of weather conditions on a specific operation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OperationalLevelCode" type="jc3iedm:ActionTaskOperationalLevelCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the operational level of the specific ACTION-TASK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!-- CandidateTargetList represented as association of AssignedTask
					<xs:choice minOccurs="0">
						<xs:element name="CandidateTargetList" type="C_BML:CandidateTargetList">
							<xs:annotation>
								<xs:documentation xml:lang="en">The CANDIDATE-TARGET-LIST.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CandidateTargetListRef" type="C_BML:CandidateTargetListRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The CANDIDATE-TARGET-LIST.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					-->
					<!-- Task Org represented as association to Order
					<xs:choice minOccurs="0">
						<xs:element name="OrganisationStructure" type="C_BML:OrganisationStructure">
							<xs:annotation>
								<xs:documentation xml:lang="en">The ORGANISATION-STRUCTURE .</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="OrganisationStructureRef" type="C_BML:OrganisationStructureRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The ORGANISATION-STRUCTURE .</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					-->
					<!--	<xs:element name="ActionTaskRuleOfEngagementInActionTask" type="C_BML:ActionTaskRuleOfEngagementInActionTask" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation xml:lang="en">A child in a 'is-constrained-by' relationship.</xs:documentation>
						</xs:annotation>
					</xs:element>-->
					<!-- Task Status reported separately
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="Status" type="C_BML:ActionTaskStatus">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'has' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="StatusRef" type="C_BML:ActionTaskStatusRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'has' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					-->
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
