	<xs:complexType name="Harbour">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY that is a restricted body of water, an anchorage, or other limited coastal water area and its water approaches from which and in which shipping operations are projected or supported.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractFacility">
				<xs:sequence>
					<xs:element name="AirportNearIndicatorCode" type="jc3iedm:HarbourAirportNearIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether an airport is near the HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApproachChannelDepthDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the depth maintained by dredging and guaranteed by the harbour authority of the specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BiologicallySecureAvailabilityIndicatorCode" type="jc3iedm:HarbourBiologicallySecureAvailabilityIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is capable of supplying biologically secure facilities, to include quarantine facilities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ConvoyMarshallingIndicatorCode" type="jc3iedm:HarbourConvoyMarshallingIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is capable of supplying convoy-marshalling facilities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DayLimitNetExplosiveQuantity" type="jc3iedm:QuantityType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum net explosive quantity that can be handled at the HARBOUR during the day. The unit of measure is kilogram.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NightLimitNetExplosiveQuantity" type="jc3iedm:QuantityType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum net explosive quantity that can be handled at the HARBOUR during the night. The unit of measure is kilogram.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DegaussingIndicatorCode" type="jc3iedm:HarbourDegaussingIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether degaussing facilities are available.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DirtyBallastIndicatorCode" type="jc3iedm:HarbourDirtyBallastIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the port has sufficient facilities for receiving oily or chemically contaminated dirty ballast.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EntranceRestrictionsIceIndicatorCode" type="jc3iedm:HarbourEntranceRestrictionsIceIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether ice is a natural factor restricting the entrance of vessels into the port.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EntranceRestrictionsSwellIndicatorCode" type="jc3iedm:HarbourEntranceRestrictionsSwellIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether heavy swell is a natural factor restricting the entrance of vessels into the port.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!-- Not computable
						<xs:element name="EntranceRestrictionsText" type="jc3iedm:TextTypeVar100" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The character string assigned to represent the factors other than tide, heavy swell, and ice restricting entrance into the port.</xs:documentation>
						</xs:annotation>
					</xs:element>-->
					<xs:element name="EstimatedTimeOfArrivalIndicatorCode" type="jc3iedm:HarbourEstimatedTimeOfArrivalIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether an estimated time of arrival message is required.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FireFightingCapabilityCode" type="jc3iedm:HarbourFireFightingCapabilityCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of fire fighting capability available at a specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FireFightingIndicatorCode" type="jc3iedm:HarbourFireFightingIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is capable of supplying fire-fighting facilities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FirstPortOfEntryIndicatorCode" type="jc3iedm:HarbourFirstPortOfEntryIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the port may be used by a vessel that needs to clear foreign goods and personnel through Customs and Immigration.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FreshWaterAvailabilityIndicatorCode" type="jc3iedm:HarbourFreshWaterAvailabilityIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is capable of providing fresh water.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LashIndicatorCode" type="jc3iedm:HarbourLashIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR can support the Lighter Aboard Ship (LASH) transportation system.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LighterageAvailabilityIndicatorCode" type="jc3iedm:HarbourLighterageAvailabilityIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the possibility for transferring goods from a ship to a wharf or another ship using a boat, usu. flat-bottomed at a specific maritime port.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumVesselDraughtDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the maximum draught of vessel that the specific HARBOUR can accommodate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumVesselLengthDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the maximum length of vessel that the specific HARBOUR can accommodate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumVesselWidthDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the maximum width of vessel that the specific HARBOUR can accommodate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MeanTidalCurrentRate" type="jc3iedm:RateType7_1" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that denotes the average velocity of the tidal flow at the specific HARBOUR. The unit of measure is kilometres per hour. The specified value must be greater than or equal to 0 (zero).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PassengerHandlingIndicatorCode" type="jc3iedm:HarbourPassengerHandlingIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is capable of handling passengers.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PersistenceCode" type="jc3iedm:HarbourPersistenceCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is permanent or temporary.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OverheadLimitsIndicatorCode" type="jc3iedm:HarbourOverheadLimitsIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether bridge and/or overhead power cables exist.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PilotageAvailabilityIndicatorCode" type="jc3iedm:HarbourPilotageAvailabilityIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether a pilot is available at the port.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PilotageRequirementIndicatorCode" type="jc3iedm:HarbourPilotageRequirementIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR requires vessels to have a pilot.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrevailingWindDirectionCode" type="jc3iedm:DirectionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the direction of the wind that most frequently occurs for the specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrevailingWindMaximumSpeedCode" type="jc3iedm:SpeedPrecisionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the maximum degree of discrimination or resolution for which the prevailing wind speed is stated.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrevailingWindMaximumSpeedRate" type="jc3iedm:RateType4_1" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that denotes the maximum recorded strength of wind at the specific HARBOUR. The unit of measure is kilometres/hour. The specified value must be greater than or equal to 0 (zero).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RefuellingAvailabilityIndicatorCode" type="jc3iedm:HarbourRefuellingAvailabilityIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR has refuelling facilities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!--  Not Computable
						<xs:element name="RefuellingLocationText" type="jc3iedm:TextTypeVar255" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The character string assigned to represent a statement of the specific refuelling details that relate to the specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>-->
					<xs:element name="RefuellingTypeCode" type="jc3iedm:HarbourRefuellingTypeCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the refuelling means available at the specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!-- Not computable
						<xs:element name="SeasonalDetailText" type="jc3iedm:TextTypeVar255" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The character string assigned to represent a statement of the specific seasonal details that relate to the specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>-->
					<xs:element name="ShelterQualityCode" type="jc3iedm:HarbourShelterQualityCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The protection provided from wind, sea, and swell in the area where normal port operations are conducted.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TankerFacilitiesIndicatorCode" type="jc3iedm:HarbourTankerFacilitiesIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the availability of facilities to process tankers at a specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TidalMeanNeapRangeDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the average range just after the first and third quarters of the moon when there is the least difference between high and low water for a specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TidalMeanSpringRangeDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the average range just after the new and full moon when there is the greatest difference between high and low water for a specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!-- Not computable 
						<xs:element name="TidalText" type="jc3iedm:TextTypeVar255" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The character string assigned to represent a statement of the specific tidal details that relate to the specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>-->
					<xs:element name="TransitAccommodationIndicatorCode" type="jc3iedm:HarbourTransitAccommodationIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is capable of supplying transit accommodation facilities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TugAvailabilityIndicatorCode" type="jc3iedm:HarbourTugAvailabilityIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the availability of tugs at a specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TurningAreaIndicatorCode" type="jc3iedm:HarbourTurningAreaIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether there is a turning basin or other water area available in a specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VehicleHandlingTypeCode" type="jc3iedm:HarbourVehicleHandlingTypeCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific types of facilities available at a specified HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HarbourRef">
		<xs:annotation>
