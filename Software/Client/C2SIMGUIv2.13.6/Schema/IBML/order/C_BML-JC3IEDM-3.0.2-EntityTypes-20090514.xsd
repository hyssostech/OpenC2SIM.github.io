	<xs:complexType name="Network">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY that provides bearer services for communication and information services and is composed of one or more links and nodes.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractFacility">
				<xs:sequence>
					<xs:element name="CategoryCode" type="jc3iedm:NetworkCategoryCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of NETWORK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SubcategoryCode" type="jc3iedm:NetworkSubcategoryCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the specific class of NETWORK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ArchitectureCode" type="jc3iedm:NetworkArchitectureCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the architecture of a specific NETWORK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ChannelCount" type="jc3iedm:CountType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the number of channels that a specific NETWORK can provide.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumCapacityRate" type="jc3iedm:RateType8_4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum data rate that a specific NETWORK can process. The unit of measure is kilobits per second (KBS). The specified value must be greater than or equal to 0 (zero).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MinimumCapacityRate" type="jc3iedm:RateType8_4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the minimum data rate that a specific NETWORK is required to process. The unit of measure is kilobits per second (KBS). The specified value must be greater than or equal to 0 (zero).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MeansCode" type="jc3iedm:NetworkMeansCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the physical linkage between the nodes of a specific NETWORK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SetNumberCount" type="jc3iedm:CountType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the frequency hopping set number parameter for this specific NETWORK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="Capacity" type="C_BML:NetworkCapacity">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'has' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CapacityRef" type="C_BML:NetworkCapacityRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'has' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="Frequency" type="C_BML:NetworkFrequency">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'uses' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="FrequencyRef" type="C_BML:NetworkFrequencyRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'uses' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="Service" type="C_BML:NetworkService">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'provides' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ServiceRef" type="C_BML:NetworkServiceRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'provides' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="NetworkRef">
