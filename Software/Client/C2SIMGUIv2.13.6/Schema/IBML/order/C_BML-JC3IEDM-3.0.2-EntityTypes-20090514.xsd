	<xs:complexType name="RadioactiveMaterielType">
		<xs:annotation>
			<xs:documentation xml:lang="en">A CONSUMABLE-MATERIEL-TYPE that is a substance which spontaneously emits radiation, and which may kill, seriously injure, or incapacitate personnel through its physiological effects or causes the deterioration of materiel.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractConsumableMaterielType">
				<xs:sequence>
					<xs:element name="CategoryCode" type="jc3iedm:RadioactiveMaterielTypeCategoryCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of RADIOACTIVE-MATERIEL-TYPE.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrimaryRadiationCode" type="jc3iedm:RadioactiveMaterielTypePrimaryRadiationCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the most intense radiation emitted by a RADIOACTIVE-MATERIEL-TYPE.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RadioactiveMaterielTypeRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some RadioactiveMaterielType - A CONSUMABLE-MATERIEL-TYPE that is a substance which spontaneously emits radiation, and which may kill, seriously injure, or incapacitate personnel through its physiological effects or causes the deterioration of materiel.</xs:documentation>
