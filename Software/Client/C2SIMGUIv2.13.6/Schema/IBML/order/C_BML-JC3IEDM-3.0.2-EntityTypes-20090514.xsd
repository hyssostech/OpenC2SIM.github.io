			<xs:extension base="C_BML:AbstractMeteorologicFeature">
				<xs:sequence>
					<xs:element name="HumidityRatio" type="jc3iedm:RatioType6_5" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric quotient value that represents the proportion of water present in the air to the maximum amount of water (saturation point) possible at a given temperature and pressure. The value must be in the range from 0 to 1.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InversionLayerCode" type="jc3iedm:AtmosphereInversionLayerCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the height of the inversion layer in the atmosphere. The stability class describes the degree of mixing of released material in the atmosphere.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PressureQuantity" type="jc3iedm:QuantityType8_4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that denotes the ambient air pressure in terms of force per unit area. The unit of measure is kPa (Kilopascals).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PressureSystemCategoryCode" type="jc3iedm:AtmospherePressureSystemCategoryCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of a pressure system in a particular ATMOSPHERE.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Temperature" type="jc3iedm:TemperatureTypeRangeTemperature5_1" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that indicates the heat of the ambient air for a specific ATMOSPHERE.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TemperatureGradientCode" type="jc3iedm:AtmosphereTemperatureGradientCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents heat change with respect to the ground and 100 m in elevation in a certain area. Acts as an indication of vertical air movement between the ground and higher elevations.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AtmosphereRef">
