	<xs:complexType name="CartesianPoint">
		<xs:annotation>
			<xs:documentation xml:lang="en">An ABSOLUTE-POINT that has its position specified in a three-dimensional Earth-centred Cartesian system.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractAbsolutePoint">
				<xs:sequence>
					<xs:element name="XCoordinateDimension" type="jc3iedm:DimensionType12_3">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the X-component of a coordinate which expresses the position of a point in a three-dimensional Cartesian coordinate system that is fixed to the earth, where the X-axis lies in the planes of the Equator and the Greenwich meridian.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="YCoordinateDimension" type="jc3iedm:DimensionType12_3">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the Y-component of a coordinate which expresses the position of a point in a three-dimensional Cartesian coordinate system that is fixed to the earth where the Y-axis is perpendicular to both the X- and Z-axes completing the right-handed coordinate system.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ZCoordinateDimension" type="jc3iedm:DimensionType12_3">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the Z-component of a coordinate which expresses the position of a point in a three-dimensional Cartesian coordinate system that is fixed to the earth, where the Z-axis coincides with the mean rotation axis of the Earth.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="XPrecisionCode" type="jc3iedm:DistancePrecisionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the resolution used for the expression of a value of a Cartesian x-coordinate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="YPrecisionCode" type="jc3iedm:DistancePrecisionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the resolution used for the expression of a value of a Cartesian y-coordinate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ZPrecisionCode" type="jc3iedm:DistancePrecisionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the resolution used for the expression of a value of a Cartesian z-coordinate.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CartesianPointRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some CartesianPoint - An ABSOLUTE-POINT that has its position specified in a three-dimensional Earth-centred Cartesian system.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractAbsolutePointRef"/>
		</xs:complexContent>
	</xs:complexType>
