	<xs:complexType name="NetworkFrequency">
		<xs:annotation>
			<xs:documentation xml:lang="en">The specification of a discrete frequency that is used on a specific NETWORK.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BandCode" type="jc3iedm:NetworkFrequencyBandCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents the frequency band of a specific NETWORK.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--<xs:element name="ChannelNumberText" type="jc3iedm:TextTypeVar6" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The character string assigned to represent the channel number associated with a specific NETWORK-FREQUENCY.</xs:documentation>
				</xs:annotation>
			</xs:element>-->
			<xs:element name="DiscreteFrequencyQuantity" type="jc3iedm:QuantityType9" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The numeric value that represents the radio frequency that a specific NETWORK may use. The unit of measure is kilohertz.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BandLowerFrequencyQuantity" type="jc3iedm:QuantityType9" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The numeric value that represents the lowest radio frequency of a specific contiguous band that a NETWORK may use. The unit of measure is kilohertz.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BandUpperFrequencyQuantity" type="jc3iedm:QuantityType9" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The numeric value that represents the highest radio frequency of a specific contiguous band that a NETWORK may use. The unit of measure is kilohertz.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EffectiveStartDatetime" type="jc3iedm:DatetimeTypeFix18">
				<xs:annotation>
					<xs:documentation xml:lang="en">The character string representing a point in time that designates the effective assignment of a specific OBJECT-TYPE-ESTABLISHMENT to a specific OBJECT-ITEM.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EffectiveEndDatetime" type="jc3iedm:DatetimeTypeFix18">
				<xs:annotation>
					<xs:documentation xml:lang="en">The character string representing a point in time that designates the end of the authorised period of effectiveness for a specific NETWORK-FREQUENCY.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ModulationCode" type="jc3iedm:NetworkFrequencyModulationCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents the modulation of a specific NETWORK.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- Not computable
				<xs:element name="PurposeText" type="jc3iedm:TextTypeVar255" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The character string assigned to represent the specific purpose of a NETWORK-FREQUENCY.</xs:documentation>
				</xs:annotation>
			</xs:element>-->
		</xs:sequence>
	</xs:complexType>
