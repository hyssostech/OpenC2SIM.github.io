	<xs:complexType name="AbstractRadioactiveEvent" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A CBRN-EVENT involving radioactive materiel(s). Concrete types are: {NuclearWeaponEvent, OtherNuclearEvent, OtherRadioactiveEvent, RadiologicalEvent}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractCbrnEvent">
				<xs:sequence>
					<xs:element name="DoseQuantity" type="jc3iedm:QuantityType14_6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the total radiation dose accumulated over the duration of the RADIOACTIVE-EVENT. The unit of measure is centiGray (cGy).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DoseRate" type="jc3iedm:RateType16_7" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that denotes the radiation dose rate. The unit of measure is centiGray per Hour (cGy/h). The specified value must be greater than or equal to 0 (zero).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DoseRateTrendCode" type="jc3iedm:RadioactiveEventDoseRateTrendCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the rate of change of the ionising radiation emitted by a radioactive materiel.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VolumeConcentrationQuantity" type="jc3iedm:QuantityType14_6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the level of radioactive contamination per unit volume in air or water. The unit of measure is Becquerels per cubic metre (BQM3).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SurfaceDepositionQuantity" type="jc3iedm:QuantityType14_6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the level of radioactive contamination per unit area on a surface. The unit of measure is Becquerels per square metre (BQM2).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RelativeDecayRateCode" type="jc3iedm:RadioactiveEventRelativeDecayRateCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the rate of decay of fallout relative to the assumed normal value of 1.2 in the Kaufmann equation.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractRadioactiveEventRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some RadioactiveEvent - A CBRN-EVENT involving radioactive materiel(s). Concrete types are: {NuclearWeaponEventRef, OtherNuclearEventRef, OtherRadioactiveEventRef, RadiologicalEventRef}</xs:documentation>
