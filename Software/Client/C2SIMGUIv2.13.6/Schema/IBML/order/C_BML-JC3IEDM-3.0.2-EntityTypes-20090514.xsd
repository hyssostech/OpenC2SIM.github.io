	<!-- Abstracts -->
	<xs:complexType name="AbstractAbsolutePoint" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A POINT in a geodetic system. Concrete types are: {CartesianPoint, GeographicPoint}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractPoint">
				<xs:sequence>
					<xs:choice minOccurs="0">
						<xs:element name="VerticalDistance" type="C_BML:VerticalDistance">
							<xs:annotation>
								<xs:documentation xml:lang="en">The vertical distance for a specific ABSOLUTE-POINT.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="VerticalDistanceRef" type="C_BML:VerticalDistanceRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The vertical distance for a specific ABSOLUTE-POINT.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractAbsolutePointRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some AbsolutePoint - A POINT in a geodetic system. Concrete types are: {CartesianPointRef, GeographicPointRef}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractPointRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractAction" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">An activity, or the occurrence of an activity, that may utilise resources and may be focused against an objective. Concrete types are: {NuclearWeaponEvent, OtherNuclearEvent, OtherRadioactiveEvent, RadiologicalEvent, ChemicalBiologicalEvent, OtherCbrnEvent, OtherActionEvent, ActionTask, Request}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NameText" type="jc3iedm:TextTypeVar50" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The character string assigned to represent a specific ACTION.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- Moved to CBML Message header
			<xs:element name="ActionContextInAction" type="C_BML:ActionContextInAction" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A child in a 'is-placed-within' relationship.</xs:documentation>
				</xs:annotation>
			</xs:element>
			-->
			<!-- Moved Functional Association to Why type
			<xs:element name="ActionFunctionalAssociationInObjectAction" type="C_BML:ActionFunctionalAssociationInObjectAction" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A child in a 'is-the-object-of' relationship.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActionFunctionalAssociationInSubjectAction" type="C_BML:ActionFunctionalAssociationInSubjectAction" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A child in a 'is-the-subject-of' relationship.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActionLocationInAction" type="C_BML:ActionLocationInAction" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A child in a 'is-geometrically-defined-through' relationship.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActionReferenceAssociationInAction" type="C_BML:ActionReferenceAssociationInAction" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A child in a 'has-relevant-information-in' relationship.</xs:documentation>
				</xs:annotation>
			</xs:element>
			-->
			<!-- Capability not required in C-BML
			<xs:element name="ActionRequiredCapabilityInAction" type="C_BML:ActionRequiredCapabilityInAction" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A child in a 'requires-as-a-minimum' relationship.</xs:documentation>
				</xs:annotation>
			</xs:element>
			-->
			<!-- Moved to Assigned When
			<xs:element name="ActionTemporalAssociationInObjectAction" type="C_BML:ActionTemporalAssociationInObjectAction" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A child in a 'is-the-object-of' relationship.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActionTemporalAssociationInSubjectAction" type="C_BML:ActionTemporalAssociationInSubjectAction" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A child in a 'is-the-subject-of' relationship.</xs:documentation>
				</xs:annotation>
			</xs:element>
			-->
			<!-- Comments not required in C-BML
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="Comment" type="C_BML:ActionComment">
					<xs:annotation>
						<xs:documentation xml:lang="en">A child in a 'has-remarks-added-by' relationship.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CommentRef" type="C_BML:ActionCommentRef">
					<xs:annotation>
						<xs:documentation xml:lang="en">A child in a 'has-remarks-added-by' relationship.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			-->
			<!--
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="Effect" type="C_BML:AbstractActionEffect">
					<xs:annotation>
						<xs:documentation xml:lang="en">A child in a 'is-measured-by' relationship.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="EffectRef" type="C_BML:AbstractActionEffectRef">
					<xs:annotation>
						<xs:documentation xml:lang="en">A child in a 'is-measured-by' relationship.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			-->
			<!-- ActionObjective is addressed in the Why
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="Objective" type="C_BML:AbstractActionObjective">
					<xs:annotation>
						<xs:documentation xml:lang="en">A child in a 'is-focussed-on' relationship.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ObjectiveRef" type="C_BML:AbstractActionObjectiveRef">
					<xs:annotation>
						<xs:documentation xml:lang="en">A child in a 'is-focussed-on' relationship.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			-->
			<!-- This association of Who type is moved at C-BML transaction level
			<xs:element name="OrganisationActionAssociationInAction" type="C_BML:OrganisationActionAssociationInAction" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A child in a 'is-acted-upon-as-specified-by' relationship.</xs:documentation>
				</xs:annotation>
			</xs:element>
			-->
			<!-- ActionResource is now moved at C-BML transactional level
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element name="Resource" type="C_BML:AbstractActionResource">
					<xs:annotation>
						<xs:documentation xml:lang="en">A child in a 'requires' relationship.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ResourceRef" type="C_BML:AbstractActionResourceRef">
					<xs:annotation>
						<xs:documentation xml:lang="en">A child in a 'requires' relationship.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			-->
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractActionRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Action - An activity, or the occurrence of an activity, that may utilise resources and may be focused against an objective. Concrete types are: {NuclearWeaponEventRef, OtherNuclearEventRef, OtherRadioactiveEventRef, RadiologicalEventRef, ChemicalBiologicalEventRef, OtherCbrnEventRef, OtherActionEventRef, ActionTaskRef, RequestRef}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractActionEffect" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A perceived effectiveness of a specific ACTION against a specific battlespace object or its class. Concrete types are: {ActionEffectItem, ActionEffectType}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DescriptionCode" type="jc3iedm:ActionEffectDescriptionCode">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents the type of outcome of a specific ACTION that is being estimated or recorded.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeverityCode" type="jc3iedm:ActionEffectSeverityCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents the degree of seriousness of a specific ACTION-EFFECT.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!--<xs:element name="ReportingData" type="C_BML:AbstractReportingData">
				<xs:annotation>
					<xs:documentation xml:lang="en">The REPORTING-DATA.</xs:documentation>
				</xs:annotation>
			</xs:element>-->
			<!--<xs:choice>
				<xs:element name="ReportingData" type="C_BML:AbstractReportingData">
					<xs:annotation>
						<xs:documentation xml:lang="en">The REPORTING-DATA.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ReportingDataRef" type="C_BML:AbstractReportingDataRef">
					<xs:annotation>
						<xs:documentation xml:lang="en">The REPORTING-DATA.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>-->
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractActionEffectRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some ActionEffect - A perceived effectiveness of a specific ACTION against a specific battlespace object or its class. Concrete types are: {ActionEffectItemRef, ActionEffectTypeRef}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractActionEvent" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">An ACTION that is an incident, phenomenon, or occasion of military significance which has occurred or is occurring but for which planning is not known. Concrete types are: {NuclearWeaponEvent, OtherNuclearEvent, OtherRadioactiveEvent, RadiologicalEvent, ChemicalBiologicalEvent, OtherCbrnEvent, OtherActionEvent}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractAction"/>
			<!-- All elements of sequence were commented out so sequence no longer required
				<xs:sequence>
					Details are not considered in C-BML
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="Detail" type="C_BML:ActionEventDetail">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'is-detailed-through' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DetailRef" type="C_BML:ActionEventDetailRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'is-detailed-through' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					Status and event are associated at the Reported Event level
					<xs:choice maxOccurs="unbounded">
						<xs:element name="Status" type="C_BML:ActionEventStatus">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'has' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="StatusRef" type="C_BML:ActionEventStatusRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'has' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
				-->
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractActionEventRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some ActionEvent - An ACTION that is an incident, phenomenon, or occasion of military significance which has occurred or is occurring but for which planning is not known. Concrete types are: {NuclearWeaponEventRef, OtherNuclearEventRef, OtherRadioactiveEventRef, RadiologicalEventRef, ChemicalBiologicalEventRef, OtherCbrnEventRef, OtherActionEventRef}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractActionRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractActionObjective" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">The focus, in terms of an OBJECT-ITEM or OBJECT-TYPE, in conducting a specific ACTION. Concrete types are: {OtherActionObjectiveType, OtherActionObjectiveItem, Target}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="QualifierCode" type="jc3iedm:ActionObjectiveQualifierCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents a restriction or other qualification applicable to a specific ACTION-OBJECTIVE for a specific ACTION.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- AuthorisingOrganisation not required in C-BML
			<xs:choice minOccurs="0">
				<xs:element name="AuthorisingOrganisation" type="C_BML:AbstractOrganisation">
					<xs:annotation>
						<xs:documentation xml:lang="en">The ORGANISATION that authorises the execution of the action against the specific ACTION-OBJECTIVE.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="AuthorisingOrganisationRef" type="C_BML:AbstractOrganisationRef">
					<xs:annotation>
						<xs:documentation xml:lang="en">The ORGANISATION that authorises the execution of the action against the specific ACTION-OBJECTIVE.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			-->
		</xs:sequence>
	</xs:complexType>
