	<xs:complexType name="AbstractVesselType" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">An EQUIPMENT-TYPE that is designed to operate on or under the water surface. Concrete types are: {OtherVesselType, SubsurfaceVesselType, SurfaceVesselType}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractEquipmentType">
				<xs:sequence>
					<xs:element name="MagneticDegaussingCodeNumberQuantity" type="jc3iedm:QuantityType12" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the peak vertical component of the magnetic field under a ship on the worst heading and at certain depth. The unit of measure is microtesla.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrismaticCoefficientRatio" type="jc3iedm:RatioType6_5" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric quotient value that represents a ratio of the volume displaced by the hull in relation to the volume of a prism or cylinder of cross section equal to the greatest cross-section of the submerged part of the hull and of length equal to the ships length between perpendiculars. The value must be in the range from 0 to 1.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DeadWeightQuantity" type="jc3iedm:QuantityType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the carrying capacity of a ship. Dead weight is the difference between the Full displacement (Gross weight) and the Light displacement (Net weight). The unit of measure is metric ton.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DraughtDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value of the distance from the Deep Water Line (DWL) to the lowest permanent projection on the hull of a vessel type including sonar domes, propellers, rudders, or other projections.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GrossRegisteredTonnageQuantity" type="jc3iedm:QuantityType9" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents a ship's internal cubic capacity or freight-carrying capacity. The unit of measure is Gross Registered Tonnage (GRT). A unit of Gross Registered Tonnage is equal to 2.83 cubic metres.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HeightAboveTheWaterlineDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the distance from the waterline to the topmost point of an unloaded vessel.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PropellerCount" type="jc3iedm:CountType2" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the number of propellers on the ship.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PropulsionTypeCode" type="jc3iedm:VesselTypePropulsionTypeCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the type of power used to move the vessel.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OperationalDisplacementQuantity" type="jc3iedm:QuantityType9" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the weight or volume of water moved by the vessel on the surface of the water. The unit of measure is metric ton.</xs:documentation>
						</xs:annotation>
					</xs:element>
