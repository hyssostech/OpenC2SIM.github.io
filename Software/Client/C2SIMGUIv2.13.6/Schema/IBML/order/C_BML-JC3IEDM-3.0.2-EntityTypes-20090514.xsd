	<xs:complexType name="Quay">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY that is a solidly constructed platform, usually parallel to the shoreline of navigable water, alongside which a vessel can be docked or berthed and, on which, the vessel can be accessed and cargo can be loaded or unloaded on one side of the vessel only.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="C_BML:AbstractFacility">
				<xs:sequence>
					<xs:element name="ContainerHandlingTypeCode" type="jc3iedm:QuayContainerHandlingTypeCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of container handling equipment available at a specific QUAY.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ContainerMaximumHandlingLengthDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the maximum length of a container that can be handled at the QUAY.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ContainerMaximumHandlingWeightQuantity" type="jc3iedm:QuantityType9" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum container weight that can be handled at the QUAY. The unit of measure is kilogram.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CraneOffloadingLiftQuantity" type="jc3iedm:QuantityType9" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum weight that can be handled by a crane at the QUAY. The unit of measure is kilogram.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CraneOffloadingTypeCode" type="jc3iedm:QuayCraneOffloadingTypeCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of crane offloading equipment available at a specific QUAY.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DayLimitNetExplosiveQuantity" type="jc3iedm:QuantityType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum net explosive quantity that can be handled at the QUAY during the day. The unit of measure is kilogram.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DraughtDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the maximum draught of vessel that the specific QUAY can accommodate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumDeadweightTonnageQuantity" type="jc3iedm:QuantityType9" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum deadweight tonnage that can be accommodated for a vessel at the specific QUAY. The unit of measure is metric ton.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NightLimitNetExplosiveQuantity" type="jc3iedm:QuantityType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum net explosive quantity that can be handled at the QUAY during the night. The unit of measure is kilogram.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RailCapacityCount" type="jc3iedm:CountType9" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the maximum number of goods/freight cars that the QUAY can accommodate at the same time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RailServedIndicatorCode" type="jc3iedm:QuayRailServedIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the QUAY has railway facilities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StorageCode" type="jc3iedm:QuayStorageCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of storage facilities available at a specific QUAY.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VesselMaximumBeamDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the maximum beam or width of vessel that the specific QUAY can accommodate.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="QuayRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Quay - A FACILITY that is a solidly constructed platform, usually parallel to the shoreline of navigable water, alongside which a vessel can be docked or berthed and, on which, the vessel can be accessed and cargo can be loaded or unloaded on one side of the vessel only.</xs:documentation>
