	<xs:complexType name="ActionContextStatus">
		<xs:annotation>
			<xs:documentation xml:lang="en">A record of the perceived state of a specific ACTION-CONTEXT as determined by the establishing organisation.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AssociationStatusCategoryCode" type="jc3iedm:AssociationStatusCategoryCode">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that indicates whether a specific ACTION-CONTEXT-STATUS refers to the beginning or termination of the association.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EstablishingOrganisationRef" type="cbml:AbstractOrganisationRef">
