<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jc3iedm="urn:int:nato:standard:mip:jc3iedm:3.0.2:oo:2.2" xmlns:cbml="http://www.sisostds.org/schemas/c-bml/1.0" targetNamespace="http://www.sisostds.org/schemas/c-bml/1.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:annotation>
		<xs:documentation xml:lang="en">XML schema defined by the Multilateral Interoperability Programme (MIP) - Editor: Dr. Michael Gerz, gerz@fgan.de, FGAN FKIE, Germany</xs:documentation>
		<xs:documentation xml:lang="en">The following schema is directly derived from the JC3IEDM 3.0.2:oo:2.2 schema.</xs:documentation>
	</xs:annotation>
	<xs:import namespace="urn:int:nato:standard:mip:jc3iedm:3.0.2:oo:2.2" schemaLocation="../../jc3iedm-simple-types/3.0.2/jc3iedm-simple-types.xsd"/>
	<xs:include schemaLocation="../../cbml-codes/1.0/cbml-codes.xsd"/>
	<xs:complexType name="AbstractAbsolutePoint" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A POINT in a geodetic system. Concrete types are: {CartesianPoint, GeographicPoint}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractPoint">
				<xs:sequence>
					<xs:choice minOccurs="0">
						<xs:element name="VerticalDistance" type="cbml:VerticalDistance">
							<xs:annotation>
								<xs:documentation xml:lang="en">The vertical distance for a specific ABSOLUTE-POINT.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="VerticalDistanceRef" type="cbml:VerticalDistanceRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">The vertical distance for a specific ABSOLUTE-POINT.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractAbsolutePointRef" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some AbsolutePoint - A POINT in a geodetic system. Concrete types are: {CartesianPointRef, GeographicPointRef}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractPointRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractAction" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">An activity, or the occurrence of an activity, that may utilise resources and may be focused against an objective. Concrete types are: {NuclearWeaponEvent, OtherNuclearEvent, OtherRadioactiveEvent, RadiologicalEvent, ChemicalBiologicalEvent, OtherCbrnEvent, OtherActionEvent, ActionTask, Request}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NameText" type="jc3iedm:TextTypeVar50" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The character string assigned to represent a specific ACTION.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractActionRef" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Action - An activity, or the occurrence of an activity, that may utilise resources and may be focused against an objective. Concrete types are: {NuclearWeaponEventRef, OtherNuclearEventRef, OtherRadioactiveEventRef, RadiologicalEventRef, ChemicalBiologicalEventRef, OtherCbrnEventRef, OtherActionEventRef, ActionTaskRef, RequestRef}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractActionEffect" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A perceived effectiveness of a specific ACTION against a specific battlespace object or its class. Concrete types are: {ActionEffectItem, ActionEffectType}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DescriptionCode" type="jc3iedm:ActionEffectDescriptionCode">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents the type of outcome of a specific ACTION that is being estimated or recorded.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SeverityCode" type="jc3iedm:ActionEffectSeverityCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents the degree of seriousness of a specific ACTION-EFFECT.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractActionEvent" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">An ACTION that is an incident, phenomenon, or occasion of military significance which has occurred or is occurring but for which planning is not known. Concrete types are: {NuclearWeaponEvent, OtherNuclearEvent, OtherRadioactiveEvent, RadiologicalEvent, ChemicalBiologicalEvent, OtherCbrnEvent, OtherActionEvent}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractAction"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractActionEventRef" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some ActionEvent - An ACTION that is an incident, phenomenon, or occasion of military significance which has occurred or is occurring but for which planning is not known. Concrete types are: {NuclearWeaponEventRef, OtherNuclearEventRef, OtherRadioactiveEventRef, RadiologicalEventRef, ChemicalBiologicalEventRef, OtherCbrnEventRef, OtherActionEventRef}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractActionRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractActionObjective" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">The focus, in terms of an OBJECT-ITEM, OBJECT-TYPE, or ACTION-TASK, in conducting a specific ACTION. Concrete types are: {OtherActionObjectiveType, OtherActionObjectiveItem, Target}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActionObjectiveQualifierCode" type="jc3iedm:ActionObjectiveQualifierCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents a restriction or other qualification applicable to a specific ACTION-OBJECTIVE for a specific ACTION.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractActionObjectiveRef" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some ActionObjective - The focus, in terms of an OBJECT-ITEM or OBJECT-TYPE, in conducting a specific ACTION. Concrete types are: {OtherActionObjectiveTypeRef, OtherActionObjectiveItemRef, TargetRef}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractActionObjectiveItem" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A battlespace object (FACILITY, FEATURE, MATERIEL, ORGANISATION or PERSON) which is the focus of a specific ACTION. Concrete types are: {OtherActionObjectiveItem, Target}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractActionObjective">
				<xs:sequence>
					<xs:element name="ActionObjectiveItemPrimacyCode" type="jc3iedm:ActionObjectiveItemPrimacyCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the relative usage of a specific ACTION-OBJECTIVE-ITEM when more than one instance of ACTION-OBJECTIVE-ITEM is designated for a specific ACTION.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CandidateTargetDetailItemRef" type="cbml:CandidateTargetDetailItemRef" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The candidate-target-detail-index that identifies specific items in a CANDIDATE-TARGET-LIST.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ObjectItemRef" type="cbml:AbstractObjectItemRef">
						<xs:annotation>
							<xs:documentation xml:lang="en">The OBJECT-ITEM. Concrete types are: {AtmosphereRef, CloudCoverRef, IcingRef, LightRef, OtherMeteorologicFeatureRef, PrecipitationRef, VisibilityRef, WindRef, AirRouteSegmentRef, OtherRouteSegmentRef, AirspaceControlMeansRef, OtherControlFeatureRef, RouteRef, GeographicFeatureRef, OtherFeatureRef, InstrumentLandingSystemRef, OtherMaterielRef, MinefieldLandRef, MinefieldMaritimeRef, OtherMilitaryObstacleRef, AirfieldRef, AnchorageRef, ApronRef, BasinRef, BerthRef, BridgeRef, DryDockRef, HarbourRef, JettyRef, OtherFacilityRef, QuayRef, RailwayRef, RoadRef, SlipwayRef, ConvoyRef, OtherOrganisationRef, UnitRef, OtherObjectItemRef, PersonRef}</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="Marking" type="cbml:ActionObjectiveItemMarking">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'is-indicated-by' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="MarkingRef" type="cbml:ActionObjectiveItemMarkingRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'is-indicated-by' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractActionObjectiveItemRef" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some ActionObjectiveItem - A battlespace object (FACILITY, FEATURE, MATERIEL, ORGANISATION or PERSON) which is the focus of a specific ACTION. Concrete types are: {OtherActionObjectiveItemRef, TargetRef}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractActionObjectiveRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractActionObjectiveType" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A class of battlespace object (FACILITY-TYPE, FEATURE-TYPE, MATERIEL-TYPE, ORGANISATION-TYPE or PERSON-TYPE) which is the focus of a specific ACTION. Concrete types are: {OtherActionObjectiveType}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractActionObjective">
				<xs:sequence>
					<xs:element name="ActionObjectiveQuantity" type="jc3iedm:QuantityType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the aggregated units of a specific ACTION-OBJECTIVE-TYPE. No unit of measure is required.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CandidateTargetDetailTypeRef" type="cbml:CandidateTargetDetailTypeRef" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The candidate-target-detail-index that identifies specific types in a CANDIDATE-TARGET-LIST.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ObjectTypeRef" type="cbml:AbstractObjectTypeRef">
						<xs:annotation>
							<xs:documentation xml:lang="en">The OBJECT-TYPE. Concrete types are: {ExecutiveMilitaryOrganisationTypeRef, MilitaryPostTypeRef, OtherMilitaryOrganisationTypeRef, TaskFormationTypeRef, UnitTypeRef, PrivateSectorOrganisationTypeRef, CivilianPostTypeRef, GroupOrganisationTypeRef, OtherOrganisationTypeRef, OtherVesselTypeRef, SubsurfaceVesselTypeRef, SurfaceVesselTypeRef, AircraftTypeRef, CbrnEquipmentTypeRef, ElectronicEquipmentTypeRef, EngineeringEquipmentTypeRef, MaritimeEquipmentTypeRef, MiscellaneousEquipmentTypeRef, RailcarTypeRef, VehicleTypeRef, WeaponTypeRef, AmmunitionTypeRef, BiologicalMaterielTypeRef, ChemicalMaterielTypeRef, OtherConsumableMaterielTypeRef, RadioactiveMaterielTypeRef, OtherMaterielTypeRef, OtherControlFeatureTypeRef, RouteTypeRef, GeographicFeatureTypeRef, OtherFeatureTypeRef, AirfieldTypeRef, BridgeTypeRef, HarbourTypeRef, MilitaryObstacleTypeRef, OtherFacilityTypeRef, OtherObjectTypeRef, PersonTypeRef}</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractActionObjectiveTypeRef" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some ActionObjectiveType - A class of battlespace object (FACILITY-TYPE, FEATURE-TYPE, MATERIEL-TYPE, ORGANISATION-TYPE or PERSON-TYPE) which is the focus of a specific ACTION. Concrete types are: {OtherActionObjectiveTypeRef}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractActionObjectiveRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractActionResource" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">An OBJECT-ITEM or an OBJECT-TYPE that is required, requested, allocated or otherwise used or planned to be used in conducting a specific ACTION. Concrete types are: {ActionResourceItem, ActionResourceType}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CriticalityIndicatorCode" type="jc3iedm:ActionResourceCriticalityIndicatorCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that denotes a judgement whether a specific resource (OBJECT-ITEM or OBJECT-TYPE) associated with a specific ACTION is essential for the effective completion of that ACTION.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ActionResourceQualifierCode" type="jc3iedm:ActionResourceQualifierCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that represents the type of restriction or other qualification applicable to a specific ACTION-RESOURCE for a specific ACTION.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Employment" type="cbml:AbstractActionResourceEmployment" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A child in a 'is-used-according-to' relationship. Concrete types are: {OtherActionResourceEmployment}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractActionResourceEmployment" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">The procedure for using a specific ACTION-RESOURCE with or without dependence upon a specific ACTION-OBJECTIVE. Concrete types are: {OtherActionResourceEmployment}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ActionObjectiveRef" type="cbml:AbstractActionObjectiveRef">
				<xs:annotation>
					<xs:documentation xml:lang="en">The ACTION-OBJECTIVE . Concrete types are: {OtherActionObjectiveTypeRef, OtherActionObjectiveItemRef, TargetRef}</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractActionTask" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">An ACTION that is being or has been planned and for which the planning details are known. Concrete types are: {ActionTask, Request}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractAction">
				<xs:sequence>
					<xs:element name="ActivityCode" type="jc3iedm:ActionTaskActivityCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the type of activity prescribed by the ACTION-TASK.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PriorityCode" type="jc3iedm:ActionTaskPriorityCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the rank of importance of a specific ACTION-TASK in view of the planning organisation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EntailedSafetyDegreeCode" type="jc3iedm:ActionTaskEntailedSafetyDegreeCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the degree of safety (or risk) entailed with an ordered operation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OvertCovertCode" type="jc3iedm:ActionTaskOvertCovertCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the property of an ACTION-TASK to be overt or covert.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MeteorologicalImpactCode" type="jc3iedm:ActionTaskMeteorologicalImpactCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents a subjective indication of the effect of weather conditions on a specific operation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OperationalLevelCode" type="jc3iedm:ActionTaskOperationalLevelCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the operational level of the specific ACTION-TASK.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractActionTaskRef" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some ActionTask - An ACTION that is being or has been planned and for which the planning details are known. Concrete types are: {ActionTaskRef, RequestRef}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractActionRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AbstractAddress" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">Precise information on the basis of which a physical or electronic destination may be accessed. Concrete types are: {ElectronicAddress, OtherAddress}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PlaceNameText" type="jc3iedm:TextTypeVar100" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The character string assigned to represent the name of the place related to the subject address.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractAddressRef" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Address - Precise information on the basis of which a physical or electronic destination may be accessed. Concrete types are: {ElectronicAddressRef, OtherAddressRef}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractAffiliation" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A specification of a country, nationality, ethnic group, functional group, exercise group, or religion to which membership or allegiance may be ascribed. Concrete types are:
{AffiliationEthnicGroup, AffiliationFunctionalGroup, AffiliationGeopolitical, AffiliationReligion, OtherAffiliation}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractAffiliationRef" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Affiliation - A specification of a country, nationality, ethnic group, functional group, exercise group, or religion to which membership or allegiance may be ascribed. Concrete types are: {AffiliationEthnicGroupRef, AffiliationFunctionalGroupRef, AffiliationGeopoliticalRef, AffiliationReligionRef, OtherAffiliationRef}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractCandidateTargetDetail" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">An element of CANDIDATE-TARGET-LIST. Concrete types are: {CandidateTargetDetailItem, CandidateTargetDetailType}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FocusTypeCode" type="jc3iedm:CandidateTargetDetailFocusTypeCode">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that denotes a general class of actions intended by the nominating authority for a specific CANDIDATE-TARGET-DETAIL.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LabelText" type="jc3iedm:TextTypeVar255" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The character string assigned to represent the identity of a specific CANDIDATE-TARGET-DETAIL in accordance with a specific scheme.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PriorityOrdinal" type="jc3iedm:OrdinalType6" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The integer value that indicates the rank of importance of a specific CANDIDATE-TARGET-DETAIL in the view of nominating authority.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SchemeCode" type="jc3iedm:CandidateTargetDetailSchemeCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The specific value that denotes an identification scheme used for recording a CANDIDATE-TARGET-DETAIL.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractCandidateTargetDetailRef" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some CandidateTargetDetail - An element of CANDIDATE-TARGET-LIST. Concrete types are: {CandidateTargetDetailItemRef, CandidateTargetDetailTypeRef}</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AbstractCbrnEvent" abstract="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">An ACTION-EVENT that involves chemical, biological, radiological or nuclear materiel individually or in combination. Concrete types are:
{NuclearWeaponEvent, OtherNuclearEvent, OtherRadioactiveEvent, RadiologicalEvent, ChemicalBiologicalEvent, OtherCbrnEvent}</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractActionEvent">
				<xs:sequence>
					<xs:element name="CbrnEventSubcategoryCode" type="jc3iedm:CbrnEventSubcategoryCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the detailed class or nature of activity prescribed by CBRN-EVENT.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AlarmResultIndicatorCode" type="jc3iedm:CbrnEventAlarmResultIndicatorCode" minOccurs="0">
						<xs:annotation>
