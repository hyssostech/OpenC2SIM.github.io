<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jc3iedm="urn:int:nato:standard:mip:jc3iedm:3.0.2:oo:2.2" xmlns:cbml="http://www.sisostds.org/schemas/c-bml/1.0" targetNamespace="http://www.sisostds.org/schemas/c-bml/1.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:annotation>
		<xs:documentation xml:lang="en">XML schema defined by the Multilateral Interoperability Programme (MIP) - Editor: Dr. Michael Gerz, gerz@fgan.de, FGAN FKIE, Germany</xs:documentation>
		<xs:documentation xml:lang="en">The following schema is directly derived from the JC3IEDM 3.0.2:oo:2.2 schema.</xs:documentation>
	</xs:annotation>
	<xs:import namespace="urn:int:nato:standard:mip:jc3iedm:3.0.2:oo:2.2" schemaLocation="../../jc3iedm-simple-types/3.0.2/jc3iedm-simple-types.xsd"/>
	<xs:include schemaLocation="../../cbml-composites/1.0/cbml-composites.xsd"/>
	<!-- Concrete Types Definition -->
	<xs:complexType name="Airfield">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY that is an area prepared for the accommodation (including any buildings, installations, or equipment) of landing and take off of aircraft.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacility">
				<xs:sequence>
					<xs:element name="AirTrafficControlPresenceIndicatorCode" type="jc3iedm:AirfieldAirTrafficControlPresenceIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that indicates whether a specific AIRFIELD provides air traffic control.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HangarAreaQuantity" type="jc3iedm:QuantityType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the total hangar area in a specific AIRFIELD. The unit of measure is square metre.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InstrumentLandingSystemPresenceIndicatorCode" type="jc3iedm:AirfieldInstrumentLandingSystemPresenceIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that indicates whether a specific AIRFIELD has an instrument landing system.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InternationalCivilAviationOrganisationText" type="jc3iedm:TextTypeVar100" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The character string assigned to represent the description the international civil aviation organization (ICAO) identifier commonly used throughout the world for reference to a known aviation facility.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VisualNavigationalAidIndicatorCode" type="jc3iedm:AirfieldVisualNavigationalAidIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value indicating whether or not the airport has a visual navigational aid displaying flashes of white or colored light to indicate the location of an airport.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AirfieldRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Airfield - A FACILITY that is an area prepared for the accommodation (including any buildings, installations, or equipment) of landing and take off of aircraft.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AirfieldType">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY-TYPE that is a class of an area prepared for the accommodation (including any buildings, installations, or equipment) of landing and take off of aircraft.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityType">
				<xs:sequence>
					<xs:element name="UseCategoryCode" type="jc3iedm:AirfieldTypeUseCategoryCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value indicating an airport's main use.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AirfieldTypeRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some AirfieldType - A FACILITY-TYPE that is a class of an area prepared for the accommodation (including any buildings, installations, or equipment) of landing and take off of aircraft.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityTypeRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AirfieldStatus">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY-STATUS that is a record of conditions of a specific AIRFIELD.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityStatus">
				<xs:sequence>
					<xs:element name="DayOperationsCode" type="jc3iedm:AirfieldStatusDayOperationsCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that indicates the status of a specific AIRFIELD to only operate during daylight.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FlightSupportCategoryCode" type="jc3iedm:AirfieldStatusFlightSupportCategoryCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that indicates the capability of a specific AIRFIELD to function under defined flight rules.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EvaluationIndicatorCode" type="jc3iedm:AirfieldStatusEvaluationIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that indicates that an AIRFIELD has been checked and its characteristics have been verified.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumNbacThroughputCount" type="jc3iedm:CountType4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the maximum count of narrow body civilian aircrafts (NBAC) that specific AIRFIELD can process per day.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumNbacParkCount" type="jc3iedm:CountType4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the maximum count of narrow body civilian aircrafts (NBAC) that can be parked at a specific AIRFIELD at one time.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumWbacThroughputCount" type="jc3iedm:CountType4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the maximum count of wide body civilian aircrafts (WBAC) that specific AIRFIELD can process per day.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumWbacParkCount" type="jc3iedm:CountType4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the maximum count of wide body civilian aircrafts (WBAC) that can be parked at a specific AIRFIELD at one time.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Bridge">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY that is a structure (including overpass and viaduct), fixed or moveable, spanning and/or providing passage over an object.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacility">
				<xs:sequence>
					<xs:element name="LongestSpanLengthDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the longest spanÂ´s length in a specific BRIDGE.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpanCount" type="jc3iedm:CountType3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the number of sections that a specific BRIDGE may have.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="UsageCode" type="jc3iedm:BridgeUsageCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the usage of a specific BRIDGE.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="BridgeRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Bridge - A FACILITY that is a structure (including overpass and viaduct), fixed or moveable, spanning and/or providing passage over an object.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="BridgeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY-TYPE that is a class of structures (including overpasses and viaducts), fixed or moveable, spanning and/or providing passage over an object.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityType">
				<xs:sequence>
					<xs:element name="DesignTypeCode" type="jc3iedm:BridgeTypeDesignTypeCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the design class of BRIDGE-TYPE.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="BridgeTypeRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some BridgeType - A FACILITY-TYPE that is a class of structures (including overpasses and viaducts), fixed or moveable, spanning and/or providing passage over an object.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityTypeRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OtherFacilityStatus">
		<xs:annotation>
			<xs:documentation xml:lang="en">An OBJECT-ITEM-STATUS that is a record of condition of a specific FACILITY.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityStatus">
				<xs:sequence>
					<xs:element name="FacilityStatusCategoryCode" type="jc3iedm:FacilityStatusCategoryCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of FACILITY-STATUS. It serves as a discriminator that partitions FACILITY-STATUS into subtypes.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Harbour">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY that is a restricted body of water, an anchorage, or other limited coastal water area and its water approaches from which and in which shipping operations are projected or supported.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacility">
				<xs:sequence>
					<xs:element name="AirportNearIndicatorCode" type="jc3iedm:HarbourAirportNearIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether an airport is near the HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApproachChannelDepthDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the depth maintained by dredging and guaranteed by the harbour authority of the specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BiologicallySecureAvailabilityIndicatorCode" type="jc3iedm:HarbourBiologicallySecureAvailabilityIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is capable of supplying biologically secure facilities, to include quarantine facilities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ConvoyMarshallingIndicatorCode" type="jc3iedm:HarbourConvoyMarshallingIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is capable of supplying convoy-marshalling facilities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DayLimitNetExplosiveQuantity" type="jc3iedm:QuantityType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum net explosive quantity that can be handled at the HARBOUR during the day. The unit of measure is kilogram.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NightLimitNetExplosiveQuantity" type="jc3iedm:QuantityType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum net explosive quantity that can be handled at the HARBOUR during the night. The unit of measure is kilogram.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DegaussingIndicatorCode" type="jc3iedm:HarbourDegaussingIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether degaussing facilities are available.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DirtyBallastIndicatorCode" type="jc3iedm:HarbourDirtyBallastIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the port has sufficient facilities for receiving oily or chemically contaminated dirty ballast.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EntranceRestrictionsIceIndicatorCode" type="jc3iedm:HarbourEntranceRestrictionsIceIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether ice is a natural factor restricting the entrance of vessels into the port.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EntranceRestrictionsSwellIndicatorCode" type="jc3iedm:HarbourEntranceRestrictionsSwellIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether heavy swell is a natural factor restricting the entrance of vessels into the port.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EstimatedTimeOfArrivalIndicatorCode" type="jc3iedm:HarbourEstimatedTimeOfArrivalIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether an estimated time of arrival message is required.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FireFightingCapabilityCode" type="jc3iedm:HarbourFireFightingCapabilityCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of fire fighting capability available at a specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FireFightingIndicatorCode" type="jc3iedm:HarbourFireFightingIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is capable of supplying fire-fighting facilities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FirstPortOfEntryIndicatorCode" type="jc3iedm:HarbourFirstPortOfEntryIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the port may be used by a vessel that needs to clear foreign goods and personnel through Customs and Immigration.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FreshWaterAvailabilityIndicatorCode" type="jc3iedm:HarbourFreshWaterAvailabilityIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is capable of providing fresh water.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LashIndicatorCode" type="jc3iedm:HarbourLashIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR can support the Lighter Aboard Ship (LASH) transportation system.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LighterageAvailabilityIndicatorCode" type="jc3iedm:HarbourLighterageAvailabilityIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the possibility for transferring goods from a ship to a wharf or another ship using a boat, usu. flat-bottomed at a specific maritime port.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumVesselDraughtDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the maximum draught of vessel that the specific HARBOUR can accommodate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumVesselLengthDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the maximum length of vessel that the specific HARBOUR can accommodate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumVesselWidthDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the maximum width of vessel that the specific HARBOUR can accommodate.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MeanTidalCurrentRate" type="jc3iedm:RateType7_1" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that denotes the average velocity of the tidal flow at the specific HARBOUR. The unit of measure is kilometres per hour. The specified value must be greater than or equal to 0 (zero).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PassengerHandlingIndicatorCode" type="jc3iedm:HarbourPassengerHandlingIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is capable of handling passengers.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PersistenceCode" type="jc3iedm:HarbourPersistenceCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is permanent or temporary.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OverheadLimitsIndicatorCode" type="jc3iedm:HarbourOverheadLimitsIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether bridge and/or overhead power cables exist.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PilotageAvailabilityIndicatorCode" type="jc3iedm:HarbourPilotageAvailabilityIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether a pilot is available at the port.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PilotageRequirementIndicatorCode" type="jc3iedm:HarbourPilotageRequirementIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR requires vessels to have a pilot.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrevailingWindDirectionCode" type="jc3iedm:DirectionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the direction of the wind that most frequently occurs for the specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrevailingWindMaximumSpeedCode" type="jc3iedm:SpeedPrecisionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the maximum degree of discrimination or resolution for which the prevailing wind speed is stated.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrevailingWindMaximumSpeedRate" type="jc3iedm:RateType4_1" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that denotes the maximum recorded strength of wind at the specific HARBOUR. The unit of measure is kilometres/hour. The specified value must be greater than or equal to 0 (zero).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RefuellingAvailabilityIndicatorCode" type="jc3iedm:HarbourRefuellingAvailabilityIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR has refuelling facilities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="RefuellingTypeCode" type="jc3iedm:HarbourRefuellingTypeCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the refuelling means available at the specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ShelterQualityCode" type="jc3iedm:HarbourShelterQualityCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The protection provided from wind, sea, and swell in the area where normal port operations are conducted.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TankerFacilitiesIndicatorCode" type="jc3iedm:HarbourTankerFacilitiesIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the availability of facilities to process tankers at a specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TidalMeanNeapRangeDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the average range just after the first and third quarters of the moon when there is the least difference between high and low water for a specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TidalMeanSpringRangeDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the average range just after the new and full moon when there is the greatest difference between high and low water for a specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TransitAccommodationIndicatorCode" type="jc3iedm:HarbourTransitAccommodationIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether the HARBOUR is capable of supplying transit accommodation facilities.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TugAvailabilityIndicatorCode" type="jc3iedm:HarbourTugAvailabilityIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the availability of tugs at a specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TurningAreaIndicatorCode" type="jc3iedm:HarbourTurningAreaIndicatorCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents whether there is a turning basin or other water area available in a specific HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VehicleHandlingTypeCode" type="jc3iedm:HarbourVehicleHandlingTypeCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific types of facilities available at a specified HARBOUR.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HarbourRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some Harbour - A FACILITY that is a restricted body of water, an anchorage, or other limited coastal water area and its water approaches from which and in which shipping operations are projected or supported.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HarbourType">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY-TYPE that is a restricted body of water, an anchorage, or other limited coastal water area and its water approaches from which and in which shipping operations are projected or supported.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityType">
				<xs:sequence>
					<xs:element name="HarbourTypeCategoryCode" type="jc3iedm:HarbourTypeCategoryCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of HARBOUR-TYPE.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HarbourTypeRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some HarbourType - A FACILITY-TYPE that is a restricted body of water, an anchorage, or other limited coastal water area and its water approaches from which and in which shipping operations are projected or supported.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityTypeRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MilitaryObstacleType">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY-TYPE that is a class of man-made devices or passive defence works that are designed to stop, impede, or divert movement of amphibious or ground forces.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityType">
				<xs:sequence>
					<xs:element name="MilitaryObstacleTypeCategoryCode" type="jc3iedm:MilitaryObstacleTypeCategoryCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of MILITARY-OBSTACLE-TYPE.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MilitaryObstacleTypeSubcategoryCode" type="jc3iedm:MilitaryObstacleTypeSubcategoryCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the detailed class of MILITARY-OBSTACLE-TYPE.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MilitaryObstacleTypeRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some MilitaryObstacleType - A FACILITY-TYPE that is a class of man-made devices or passive defence works that are designed to stop, impede, or divert movement of amphibious or ground forces.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityTypeRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MinefieldLand">
		<xs:annotation>
			<xs:documentation xml:lang="en">A MINEFIELD that is an area of land containing mines.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMinefield">
				<xs:sequence>
					<xs:element name="DepthPlacementCode" type="jc3iedm:MinefieldLandDepthPlacementCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that indicates the placement of mines with respect to the surface.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MinefieldLandFunctionCode" type="jc3iedm:MinefieldLandFunctionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the intended function of the specific MINEFIELD-LAND.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PatternCode" type="jc3iedm:MinefieldLandPatternCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the pattern of a specific MINEFIELD-LAND.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PersistenceCode" type="jc3iedm:MinefieldLandPersistenceCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the option for terminating the effectiveness of a specific MINEFIELD-LAND.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StoppingPowerCode" type="jc3iedm:MinefieldLandStoppingPowerCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the stopping power of a particular MINEFIELD-LAND.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MinefieldLandRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some MinefieldLand - A MINEFIELD that is an area of land containing mines.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMinefieldRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MinefieldMaritime">
		<xs:annotation>
			<xs:documentation xml:lang="en">A MINEFIELD that is an area or volume of water containing mines.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMinefield">
				<xs:sequence>
					<xs:element name="DepthPlacementCode" type="jc3iedm:MinefieldMaritimeDepthPlacementCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that indicates the intended depth placement of maritime mines.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ExpectedVesselTransitCount" type="jc3iedm:CountType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the expected number of transiting vessels.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MinefieldMaritimeFunctionCode" type="jc3iedm:MinefieldMaritimeFunctionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the intended function of a specific MINEFIELD-MARITIME.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MmoeInitialThreatProbabilityRatio" type="jc3iedm:RatioType6_5" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric quotient value that represents the planned or estimated likelihood that the first target ship to enter the minefield will be a casualty. MMOE stands for Minefield Measurement Of Effectiveness. The value must be in the range from 0 to 1.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DetectionProbabilityRatio" type="jc3iedm:RatioType6_5" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric quotient value that represents the probability that a mine will be located during MCM operations. The value must be in the range from 0 to 1.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="LifeDuration" type="jc3iedm:DurationType19" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents a quantity of time in milliseconds for the estimated life of the MINEFIELD-MARITIME.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MineDetailedText" type="jc3iedm:TextTypeVar50" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The character string assigned to represent a description of a specific mine.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MinesLaidCount" type="jc3iedm:CountType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the number of maritime mines laid in a specific MINEFIELD-MARITIME.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BottomNaturalCamouflageCode" type="jc3iedm:MinefieldMaritimeBottomNaturalCamouflageCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the description of the ground of a lake, river, or ocean, or other body of water with respect to the ability to hide mines on the bottom at a specific MINEFIELD-MARITIME.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SubfunctionCode" type="jc3iedm:MinefieldMaritimeSubfunctionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the intended purpose of the MINEFIELD-MARITIME.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="CasualtyEstimate" type="cbml:MinefieldMaritimeCasualtyEstimate">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'has' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="CasualtyEstimateRef" type="cbml:MinefieldMaritimeCasualtyEstimateRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'has' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="SustainedThreatMeasureOfEffectiveness" type="cbml:MinefieldMaritimeSustainedThreatMeasureOfEffectiveness">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'has' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="SustainedThreatMeasureOfEffectivenessRef" type="cbml:MinefieldMaritimeSustainedThreatMeasureOfEffectivenessRef">
							<xs:annotation>
								<xs:documentation xml:lang="en">A child in a 'has' relationship.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MinefieldMaritimeRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some MinefieldMaritime - A MINEFIELD that is an area or volume of water containing mines.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMinefieldRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MinefieldMaritimeStatus">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY-STATUS that is a record of condition of a specific MINEFIELD-MARITIME.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityStatus">
				<xs:sequence>
					<xs:element name="MinefieldMaritimeStatusCode" type="jc3iedm:MinefieldMaritimeStatusCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the known status of a MINEFIELD-MARITIME.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ColourCode" type="jc3iedm:MinefieldMaritimeStatusColourCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the known status of mined channels of a MINEFIELD-MARITIME.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ExpectedLevelMcmCode" type="jc3iedm:MinefieldMaritimeStatusExpectedLevelMcmCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific values of expected mine countermeasures (MCM) to be employed against a MINEFIELD-MARITIME.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MinesDetectedQuantity" type="jc3iedm:QuantityType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value representing the number of mines detected in a specific MINEFIELD-MARITIME.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MinesDetectedCount" type="jc3iedm:CountType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the number of mines detected in a specific MINEFIELD-MARITIME at the time of the report.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MineZoneRiskCode" type="jc3iedm:MinefieldMaritimeStatusMineZoneRiskCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the known threat (risk) of a MINEFIELD-MARITIME.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SeedingCode" type="jc3iedm:MinefieldMaritimeStatusSeedingCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that indicates the seeding details for a MINEFIELD-MARITIME.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SweptDepthQuantity" type="jc3iedm:QuantityType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value representing the depth below the surface to which minesweeping has taken place. The unit of measure is metres.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ThreatRatio" type="jc3iedm:RatioType6_5" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric quotient value that represents the current percentage threat to the enemy of a specific MINEFIELD-MARITIME. The value must be in the range from 0 to 1.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MineDetectionCode" type="jc3iedm:MinefieldMaritimeStatusMineDetectionCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the status of the means of detection of a mine in a MINEFIELD-MARITIME.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MinesCount" type="jc3iedm:CountType6" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the number of mines within a specific maritime minefield at the time of the report.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MinefieldMaritimeCasualtyEstimate">
		<xs:annotation>
			<xs:documentation xml:lang="en">An estimate of the average number of casualties for a given number of vessel transits through a specific MINEFIELD-MARITIME.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AverageCount" type="jc3iedm:CountType6" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The integer value representing the planned or estimated number of casualties for a specific MINEFIELD-MARITIME-CASUALTY-ESTIMATE that would occur as an average in a large number of transit attempts.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="GivenTransitCount" type="jc3iedm:CountType6" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The integer value representing the given number of transits for a specific MINEFIELD-MARITIME-CASUALTY-ESTIMATE.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MinefieldMaritimeCasualtyEstimateRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some MinefieldMaritimeCasualtyEstimate - An estimate of the average number of casualties for a given number of vessel transits through a specific MINEFIELD-MARITIME.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MinefieldMaritimeSustainedThreatMeasureOfEffectiveness">
		<xs:annotation>
			<xs:documentation xml:lang="en">A measure of effectiveness for a specific MINEFIELD-MARITIME in terms of probability of mine function against a transit vessel over a given period of time.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PlannedDuration" type="jc3iedm:DurationType19" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The numeric value that represents a quantity of time in milliseconds for the planned or estimated duration for a specific MINEFIELD-MARITIME-SUSTAINED-THREAT-MEASURE-OF-EFFECTIVENESS.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProbabilityRatio" type="jc3iedm:RatioType6_5" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">The numeric quotient value that represents the planned or estimated likelihood of threat over an extended period for a specific MINEFIELD-MARITIME-SUSTAINED-THREAT-MEASURE-OF-EFFECTIVENESS. The value must be in the range from 0 to 1.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MinefieldMaritimeSustainedThreatMeasureOfEffectivenessRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some MinefieldMaritimeSustainedThreatMeasureOfEffectiveness - A measure of effectiveness for a specific MINEFIELD-MARITIME in terms of probability of mine function against a transit vessel over a given period of time.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OID" type="jc3iedm:OIDType">
				<xs:annotation>
					<xs:documentation xml:lang="en">The globally unique object identifier. An OID can be any globally unique string (URL, GUID...).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OtherFacility">
		<xs:annotation>
			<xs:documentation xml:lang="en">An OBJECT-ITEM that is built, installed or established to serve some particular purpose and is identified by the service it provides rather than by its content.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacility"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OtherFacilityRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some OtherFacility - An OBJECT-ITEM that is built, installed or established to serve some particular purpose and is identified by the service it provides rather than by its content.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OtherFacilityType">
		<xs:annotation>
			<xs:documentation xml:lang="en">An OBJECT-TYPE that is intended to be built, installed or established to serve some particular purpose and is identified by the service it is intended to provide rather than by its content.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityType">
				<xs:sequence>
					<xs:element name="FacilityTypeCategoryCode" type="jc3iedm:FacilityTypeCategoryCode">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the class of FACILITY-TYPE. It serves as a discriminator that partitions FACILITY-TYPE into subtypes.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OtherFacilityTypeRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some OtherFacilityType - An OBJECT-TYPE that is intended to be built, installed or established to serve some particular purpose and is identified by the service it is intended to provide rather than by its content.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacilityTypeRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OtherMilitaryObstacle">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY designed to stop, impede, or divert movement of amphibious or ground forces.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMilitaryObstacle"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OtherMilitaryObstacleRef">
		<xs:annotation>
			<xs:documentation xml:lang="en">A reference to some OtherMilitaryObstacle - A FACILITY designed to stop, impede, or divert movement of amphibious or ground forces.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractMilitaryObstacleRef"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Railway">
		<xs:annotation>
			<xs:documentation xml:lang="en">A FACILITY that is a track or set of tracks made of steel rails along which trains run.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="cbml:AbstractFacility">
				<xs:sequence>
					<xs:element name="TrackGaugeCode" type="jc3iedm:RailwayTrackGaugeCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the distance between the internal sides of rails on a RAILWAY line.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TrackCount" type="jc3iedm:CountType4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the number of tracks on a RAILWAY line.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TrainDensityCount" type="jc3iedm:CountType4" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The integer value representing the maximum number of trains, which can be moved in each direction over a specified section of track in a 24 hour period.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BlockDistanceDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the minimum length of the passing track intervals for single track lines or the safety margin related to the signalling system on double or multiple track lines.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SleeperDensityDimension" type="jc3iedm:DimensionType12_3" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The one-dimensional linear distance representing the average horizontal distance measured from side to side and perpendicular to the central axis of the gap between two RAILWAY sleepers (ties).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GrossTrailingLoadQuantity" type="jc3iedm:QuantityType9" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that represents the maximum tonnage that can be moved under optimum conditions. The unit of measure is metric ton.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MaximumSpeedRate" type="jc3iedm:RateType4_1" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The numeric value that denotes the maximum speed that it is possible to move on a specific RAILWAY. The unit of measure is kilometres per hour. The specified value must be greater than or equal to 0 (zero).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TractionSystemCode" type="jc3iedm:RailwayTractionSystemCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the motive power (engine type) that is supported along a specific RAILWAY.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SignalSystemCode" type="jc3iedm:RailwaySignalSystemCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the type of signal system used for the RAILWAY.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SignalSystemEfficiencyCode" type="jc3iedm:RailwaySignalSystemEfficiencyCode" minOccurs="0">
						<xs:annotation>
							<xs:documentation xml:lang="en">The specific value that represents the percentage value used to compute the inevitable delays caused by the type of signalling in use on the RAILWAY line.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
